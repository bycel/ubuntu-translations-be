#: src/nautilus-share.c:272
#, c-format
msgid "Nautilus needs to add some permissions to your folder \"%s\" in order to share it"
msgstr "Nautilus патрабуе дадавання некаторых дазволаў да вашай папкі «%s», каб наладзіць абагулены доступ да яе"

#: src/nautilus-share.c:280
#, c-format
msgid "The folder \"%s\" needs the following extra permissions for sharing to work:\n"
"%s%s%sDo you want Nautilus to add these permissions to the folder automatically?"
msgstr "Папка «%s» патрабуе наступныя дадатковыя дазволы для абагульвання доступу:\n"
"%s%s%sВы сапраўды хочаце, каб Nautilus дадаў гэтыя дазволы ў папку аўтаматычна?"

#: src/nautilus-share.c:284
msgid "  - read permission by others\n"
msgstr "  - дазвол на чытанне іншымі карыстальнікамі"

#: src/nautilus-share.c:285
msgid "  - write permission by others\n"
msgstr "  - дазвол на запіс іншымі карыстальнікамі\n"

#: src/nautilus-share.c:286
msgid "  - execute permission by others\n"
msgstr "  - дазвол на выкананне іншымі карыстальнікамі"

#: src/nautilus-share.c:289
msgid "Cancel"
msgstr "Скасаваць"

#: src/nautilus-share.c:290
msgid "Add the permissions automatically"
msgstr "Дадаваць дазволы аўтаматычна"

#: src/nautilus-share.c:326
#, c-format
msgid "Could not change the permissions of folder \"%s\""
msgstr "Не ўдалося змяніць дазволы папкі «%s»"

#: src/nautilus-share.c:566
msgid "Share name is too long"
msgstr "Вельмі доўгая назва для абагульвання"

#: src/nautilus-share.c:605
msgid "The share name cannot be empty"
msgstr "Назва для абагуленага доступу не можа быць пустой"

#: src/nautilus-share.c:618
#, c-format
msgid "Error while getting share information: %s"
msgstr ""

#: src/nautilus-share.c:628
msgid "Another share has the same name"
msgstr "Абагульванне з такой назва ўжо існуе"

#: src/nautilus-share.c:675 src/nautilus-share.c:912
msgid "Modify _Share"
msgstr "Змяніць _абагульванне"

#: src/nautilus-share.c:675 src/nautilus-share.c:914
msgid "Create _Share"
msgstr "Стварыць _абагульванне"

#: src/nautilus-share.c:803
msgid "There was an error while getting the sharing information"
msgstr "Памылка падчас атрымання звестак абагульвання"

#: src/nautilus-share.c:1183
msgid "Share"
msgstr "Абагуліць"

#: src/nautilus-share.c:1247
msgid "Folder Sharing"
msgstr "Абагульванне папкі"

#: src/nautilus-share.c:1295
msgid "Sharing Options"
msgstr "Параметры абагульвання"

#: src/nautilus-share.c:1296
msgid "Share this Folder"
msgstr "Абагуліць гэтую папку"

#: src/shares.c:125
#, c-format
msgid "%s %s %s returned with signal %d"
msgstr "%s %s %s вярнулася з сігналам %d"

#: src/shares.c:134
#, c-format
msgid "%s %s %s failed for an unknown reason"
msgstr "Збой %s %s %s па невядомай прычыне"

#: src/shares.c:154
#, c-format
msgid "'net usershare' returned error %d: %s"
msgstr "'net usershare' вярнуў памылку %d: %s"

#: src/shares.c:156
#, c-format
msgid "'net usershare' returned error %d"
msgstr "'net usershare' вярнуў памылку %d"

#: src/shares.c:187
#, c-format
msgid "the output of 'net usershare' is not in valid UTF-8 encoding"
msgstr "вывод «net usershare» з'яўляецца памылковым кадзіраваннем UTF-8"

#: src/shares.c:442 src/shares.c:616
#, c-format
msgid "Failed"
msgstr "Збой"

#: src/shares.c:550
#, c-format
msgid "Samba's testparm returned with signal %d"
msgstr "Праграма testparm з Samba завяршыла сваю працу з сігналам:  %d"

#: src/shares.c:556
#, c-format
msgid "Samba's testparm failed for an unknown reason"
msgstr "Збой праграмы testparm з Samba. Прычына невядома"

#: src/shares.c:571
#, c-format
msgid "Samba's testparm returned error %d: %s"
msgstr "Праграма testparm з Samba вярнула памылку %d: %s"

#: src/shares.c:573
#, c-format
msgid "Samba's testparm returned error %d"
msgstr "Праграма testparm з Samba вярнула памылку %d"

#: src/shares.c:684
#, c-format
msgid "Cannot remove the share for path %s: that path is not shared"
msgstr "Немагчыма выдаліць абагульванне для шляху %s: гэты шлях не абагулены"

#: src/shares.c:730
#, c-format
msgid "Cannot change the path of an existing share; please remove the old share first and add a new one"
msgstr ""

#: interfaces/share-dialog.ui:27
msgid "<big><b>Folder Sharing</b></big>"
msgstr "<big><b>Абагульванне папкі</b></big>"

#: interfaces/share-dialog.ui:75
msgid "Share _name:"
msgstr "Абагуліць _назву:"

#: interfaces/share-dialog.ui:108
msgid "Share this _folder"
msgstr "Абагуліць гэту _папку"

#: interfaces/share-dialog.ui:139
msgid "Co_mment:"
msgstr "_Каментарый:"

#: interfaces/share-dialog.ui:170
msgid "_Allow others to create and delete files in this folder"
msgstr "_Дазволіць іншым карыстальнікам ствараць і выдаляць файлы ў гэтай папцы"

#: interfaces/share-dialog.ui:187
msgid "_Guest access (for people without a user account)"
msgstr "_Гасцявы доступ (для людзей, якія не маюць уліковага запісу карыстальніка)"

