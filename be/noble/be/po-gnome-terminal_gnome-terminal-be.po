#: src/org.gnome.Terminal.gschema.xml:399
msgid "Keyboard shortcut to close a tab"
msgstr "Спалучэнне клавіш, каб закрыць укладку"

#: src/org.gnome.Terminal.gschema.xml:403
msgid "Keyboard shortcut to close a window"
msgstr "Спалучэнне клавіш, каб закрыць акно"

#: src/org.gnome.Terminal.gschema.xml:407
msgid "Keyboard shortcut to copy text"
msgstr "Спалучэнне клавіш, каб скапіяваць тэкст"

#: src/org.gnome.Terminal.gschema.xml:411
msgid "Keyboard shortcut to copy text as HTML"
msgstr "Спалучэнне клавіш, каб скапіяваць тэкст як HTML"

#: src/org.gnome.Terminal.gschema.xml:415
msgid "Keyboard shortcut to paste text"
msgstr "Спалучэнне клавіш, каб уставіць тэкст"

#: src/org.gnome.Terminal.gschema.xml:419
msgid "Keyboard shortcut to select all text"
msgstr ""

#: src/org.gnome.Terminal.gschema.xml:423
msgid "Keyboard shortcut to open the Preferences dialog"
msgstr ""

#: src/org.gnome.Terminal.gschema.xml:427
msgid "Keyboard shortcut to toggle full screen mode"
msgstr "Спалучэнне клавіш, каб пераключыць поўнаэкранны рэжым"

#: src/org.gnome.Terminal.gschema.xml:431
msgid "Keyboard shortcut to toggle the visibility of the menubar"
msgstr "Спалучэнне клавіш, каб пераключыць бачнасць радка меню"

#: src/org.gnome.Terminal.gschema.xml:435
msgid "Keyboard shortcut to toggle the read-only state"
msgstr "Спалучэнне клавіш, каб пераключыць стан «толькі для чытання»"

#: src/org.gnome.Terminal.gschema.xml:439
msgid "Keyboard shortcut to reset the terminal"
msgstr ""

#: src/org.gnome.Terminal.gschema.xml:443
msgid "Keyboard shortcut to reset and clear the terminal"
msgstr "Спалучэнне клавіш, каб скінуць налады і ачысціць тэрмінал"

#: src/org.gnome.Terminal.gschema.xml:447
msgid "Keyboard shortcut to open the search dialog"
msgstr "Спалучэнне клавіш, каб адкрыць дыялогавае акно пошуку"

#: src/org.gnome.Terminal.gschema.xml:451
msgid "Keyboard shortcut to find the next occurrence of the search term"
msgstr ""

#: src/org.gnome.Terminal.gschema.xml:455
msgid "Keyboard shortcut to find the previous occurrence of the search term"
msgstr ""

#: src/org.gnome.Terminal.gschema.xml:459
msgid "Keyboard shortcut to clear the find highlighting"
msgstr "Спалучэнне клавіш, каб прыбраць падсветку знойдзенага"

#: src/org.gnome.Terminal.gschema.xml:463
msgid "Keyboard shortcut to switch to the previous tab"
msgstr "Спалучэнне клавіш, каб пераключыцца на папярэднюю ўкладку"

#: src/org.gnome.Terminal.gschema.xml:467
msgid "Keyboard shortcut to switch to the next tab"
