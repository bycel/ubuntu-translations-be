#: properties/nm-pptp-dialog.ui:451
msgid "Show password"
msgstr "Паказаць пароль"

#: properties/nm-pptp-dialog.ui:468
msgid "NT Domain"
msgstr "Дамен NT"

#: properties/nm-pptp-dialog.ui:480
msgid "Append the domain name <domain> to the local host name for authentication purposes.\n"
"config: domain <domain>"
