#. Translators, please edit/rearrange these strftime(3) tokens to suit your locale!
#. Format string for the day on the first menuitem in the datetime indicator.
#. This format string gives the full weekday, date, month, and year.
#. en_US example: "%A, %B %e %Y" --> Saturday, October 31 2020"
#. en_GB example: "%A, %e %B %Y" --> Saturday, 31 October 2020"
#: ../src/menu.cpp:329
msgid "%A, %e %B %Y"
msgstr "%A, %e %B %Y"

#. add the 'Add Event…' menuitem
#: ../src/menu.cpp:429
msgid "Add Event…"
msgstr "Дадаць падзею…"

#: ../src/menu.cpp:439
msgid "Clock"
msgstr "Гадзіннік"

#: ../src/menu.cpp:491
msgid "Time & Date Settings…"
msgstr "Налады даты і часу…"

#: ../src/menu.cpp:546
msgid "Date and Time"
msgstr "Дата і час"

#: ../src/menu.cpp:596
msgid "Time & Date"
msgstr "Час і дата"

#: ../src/menu.cpp:601
#, c-format
msgid "%s (has alarms)"
msgstr "%s (з будзільнікам)"

#. * strftime(3) format for abbreviated weekday,
#. hours, minutes in a 12h locale; e.g. Wed, 2:00 PM
#: ../src/snap.cpp:144
msgid "%a, %l:%M %p"
msgstr "%a, %H:%M%a, %H:%M"

#. * A strftime(3) format for abbreviated weekday,
#. hours, minutes in a 24h locale; e.g. Wed, 14:00
#: ../src/snap.cpp:148
msgid "%a, %H:%M"
msgstr "%a, %H:%M"

#: ../src/snap.cpp:153
#, c-format
msgid "Alarm %s"
msgstr "Будзільнік %s"

#: ../src/snap.cpp:154
#, c-format
msgid "Event %s"
msgstr "Падзея %s"

#: ../src/snap.cpp:162 ../src/snap.cpp:166
msgid "OK"
msgstr "Добра"

#: ../src/snap.cpp:163
msgid "Snooze"
msgstr "Адкласці"
