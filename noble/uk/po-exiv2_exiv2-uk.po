#: src/exiv2.cpp:323
msgid "             k : key\n"
msgstr "             k : ключ\n"

#: src/exiv2.cpp:324
msgid "             l : tag label\n"
msgstr "             l : мітка тегу\n"

#: src/exiv2.cpp:325
msgid "             n : tag name\n"
msgstr "             n : ім’я тегу\n"

#: src/exiv2.cpp:326
msgid "             y : type\n"
msgstr "             y : тип\n"

#: src/exiv2.cpp:327
msgid "             c : number of components (count)\n"
msgstr "             c : кількість компонентів (count)\n"

#: src/exiv2.cpp:328
