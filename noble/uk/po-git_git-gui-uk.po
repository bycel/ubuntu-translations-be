#: lib/merge.tcl:137
msgid "Merge completed successfully."
msgstr "Злиття успішно виконано."

#: lib/merge.tcl:139
msgid "Merge failed.  Conflict resolution is required."
msgstr "Помилка злиття. Потрібне вирішення конфліктів."

#: lib/merge.tcl:156
#, tcl-format
msgid "%s (%s): Merge"
msgstr ""

#: lib/merge.tcl:164
#, tcl-format
msgid "Merge Into %s"
msgstr "Злити з %s"

#: lib/merge.tcl:183
