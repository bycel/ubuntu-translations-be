#. FIXME: Sort P and Q by line number.
#: fortran/resolve.c:7845
#, gcc-internal-format, gfc-internal-format
msgid "CASE label at %L overlaps with CASE label at %L"
msgstr "Мітка CASE в %L перетинається з міткою CASE в %L"

#: fortran/resolve.c:7896
#, gcc-internal-format, gfc-internal-format
msgid "Expression in CASE statement at %L must be of type %s"
msgstr "Вираз в операторі CASE в %L має бути типу %s"

#: fortran/resolve.c:7907
#, gcc-internal-format, gfc-internal-format
msgid "Expression in CASE statement at %L must be of kind %d"
msgstr "Вираз в операторі CASE в %L має бути виду %d"

#: fortran/resolve.c:7920
#, gcc-internal-format, gfc-internal-format
msgid "Expression in CASE statement at %L must be scalar"
msgstr "Вираз в операторі CASE в %L має бути скаляром"

#: fortran/resolve.c:7966
#, gcc-internal-format, gfc-internal-format
msgid ""
"Selection expression in computed GOTO statement at %L must be a scalar "
"integer expression"
msgstr ""
"Вираз вибору в обчислюваному операторі GOTO в %L має бути скалярним "
"цілочисельним виразом"

#: fortran/resolve.c:7985
#, gcc-internal-format, gfc-internal-format
msgid "Argument of SELECT statement at %L cannot be %s"
msgstr "Аргумент оператора SELECT в %L не може бути %s"

#: fortran/resolve.c:7995
#, gcc-internal-format, gfc-internal-format
msgid "Argument of SELECT statement at %L must be a scalar expression"
msgstr "Аргумент оператора SELECT в %L має бути скалярним виразом"

#: fortran/resolve.c:8013 fortran/resolve.c:8021
#, gcc-internal-format, gfc-internal-format
msgid "Expression in CASE statement at %L is not in the range of %s"
msgstr "Вираз в операторі CASE в %L не знаходиться в діапазоні %s"

#: fortran/resolve.c:8083 fortran/resolve.c:8625
#, gcc-internal-format, gfc-internal-format
msgid ""
"The DEFAULT CASE at %L cannot be followed by a second DEFAULT CASE at %L"
msgstr "За DEFAULT CASE в %L не може йти другий DEFAULT CASE в %L"

#: fortran/resolve.c:8109
#, gcc-internal-format, gfc-internal-format
msgid "Logical range in CASE statement at %L is not allowed"
msgstr "Логічний діапазон в операторі CASE в %L неприпустимий"

#: fortran/resolve.c:8121
#, gcc-internal-format, gfc-internal-format
msgid "Constant logical value in CASE statement is repeated at %L"
msgstr "Константне логічне значення в операторі CASE повторюється в %L"

#: fortran/resolve.c:8136
#, gcc-internal-format, gfc-internal-format
msgid "Range specification at %L can never be matched"
msgstr "Не може бути відповідності специфікації діапазону в %L"

#: fortran/resolve.c:8239
#, gcc-internal-format, gfc-internal-format
msgid "Logical SELECT CASE block at %L has more that two cases"
msgstr "Логічний блок SELECT CASE в %L має понад два випадки"

#: fortran/resolve.c:9030
#, gcc-internal-format
msgid "Associate-name %qs at %L is used as array"
msgstr "Associate-name %qs в %L використовується як масив"

#: fortran/resolve.c:8325
#, gcc-internal-format, gfc-internal-format
msgid "CLASS selector at %L needs a temporary which is not yet implemented"
msgstr "Селектор CLASS в %L потребує тимчасового, який ще не реалізований"

#: fortran/resolve.c:8518
#, gcc-internal-format, gfc-internal-format
msgid "Selector shall be polymorphic in SELECT TYPE statement at %L"
msgstr "Селектор в операторі SELECT TYPE  в %L має бути поліморфним"

#: fortran/resolve.c:8535 fortran/resolve.c:8547
#, gcc-internal-format, gfc-internal-format
msgid "Selector at %L must not be coindexed"
msgstr "Селектор в %L має бути коіндексованим"

#: fortran/resolve.c:8576
#, gcc-internal-format, gfc-internal-format
msgid "TYPE IS at %L overlaps with TYPE IS at %L"
msgstr "TYPE IS в %L перетинається з TYPE IS в %L"

#: fortran/resolve.c:8588
#, gcc-internal-format
msgid "Derived type %qs at %L must be extensible"
msgstr "Похідний тип %qs в %L має бути розширюваним"

#: fortran/resolve.c:8600
#, gcc-internal-format
msgid "Derived type %qs at %L must be an extension of %qs"
msgstr "Похідний тип %qs в %L має бути розширенням для  %qs"

#: fortran/resolve.c:8603
#, gcc-internal-format
msgid "Unexpected intrinsic type %qs at %L"
msgstr "Неочікуваний внутрішній тип %qs в %L"

#: fortran/resolve.c:8613
#, gcc-internal-format, gfc-internal-format
msgid ""
"The type-spec at %L shall specify that each length type parameter is assumed"
msgstr ""
"Специфікатор типу %L повинен зазначати, що кожний параметр типу довжини є "
"передбачуваним"

#: fortran/resolve.c:8851
#, gcc-internal-format, gfc-internal-format
msgid "Double CLASS IS block in SELECT TYPE statement at %L"
msgstr "Подвійний блок CLASS IS в операторі SELECT TYPE в %L"

#: fortran/resolve.c:8947
#, gcc-internal-format, gfc-internal-format
msgid "Invalid context for NULL () intrinsic at %L"
msgstr "Неприпустимий контекст для NULL () внутрішньої в %L"

#: fortran/resolve.c:9010
#, gcc-internal-format, gfc-internal-format
msgid "DTIO %s procedure at %L must be recursive"
msgstr "Процедура DTIO %s в %L має бути рекурсивною"

#: fortran/resolve.c:9020
#, gcc-internal-format, gfc-internal-format
msgid ""
"Data transfer element at %L cannot be polymorphic unless it is processed by "
"a defined input/output procedure"
msgstr ""
"Елемент передавання даних в %L не може бути поліморфних, якщо він не "
"обробляється за допомогою визначеної процедури введення/виведення"

#: fortran/resolve.c:9033
#, gcc-internal-format, gfc-internal-format
msgid ""
"Data transfer element at %L cannot have POINTER components unless it is "
"processed by a defined input/output procedure"
msgstr ""
"Елемент передавання даних в %L не може мати компоненти POINTER, якщо він не "
"обробляється за допомогою визначеної процедури введення/виведення"

#: fortran/resolve.c:9042
#, gcc-internal-format, gfc-internal-format
msgid "Data transfer element at %L cannot have procedure pointer components"
msgstr ""
"Елемент передавання даних в %L не може мати компоненти вказівника на "
"процедуру"

#: fortran/resolve.c:9049
#, gcc-internal-format, gfc-internal-format
msgid ""
"Data transfer element at %L cannot have ALLOCATABLE components unless it is "
"processed by a defined input/output procedure"
msgstr ""
"Елемент передавання даних в %L не може мати компоненти ALLOCATABLE, якщо він "
"не обробляється за допомогою визначеної процедури введення/виведення"

#: fortran/resolve.c:9060
#, gcc-internal-format, gfc-internal-format
msgid "Data transfer element at %L cannot have PRIVATE components"
msgstr "Елемент передавання даних в %L не може мати компоненти PRIVATE"

#: fortran/resolve.c:9066
#, gcc-internal-format, gfc-internal-format
msgid ""
"Data transfer element at %L cannot have PRIVATE components unless it is "
"processed by a defined input/output procedure"
msgstr ""
"Елемент передавання даних в %L не може мати компоненти PRIVATE, якщо він не "
"обробляється за допомогою визначеної процедури введення/виведення"

#: fortran/resolve.c:9081
#, gcc-internal-format, gfc-internal-format
msgid ""
"Data transfer element at %L cannot be a full reference to an assumed-size "
"array"
msgstr ""
"Елемент передавання даних в %L не може бути повним посилання на масив "
"передбачуваного типу"

#: fortran/resolve.c:9138
#, gcc-internal-format, gfc-internal-format
msgid "Lock variable at %L must be a scalar of type LOCK_TYPE"
msgstr "Змінна Lock в %L має бути скаляром типу LOCK_TYPE"

#: fortran/resolve.c:9148
#, gcc-internal-format, gfc-internal-format
msgid "Event variable at %L must be a scalar of type EVENT_TYPE"
msgstr "Змінна Event в %L має бути скаляром типу EVENT_TYPE"

#: fortran/resolve.c:9152
#, gcc-internal-format, gfc-internal-format
msgid "Event variable argument at %L must be a coarray or coindexed"
msgstr "Аргумент змінної Event в %L має бути комасивом або коіндексованим"

#: fortran/resolve.c:9155
#, gcc-internal-format, gfc-internal-format
msgid "Event variable argument at %L must be a coarray but not coindexed"
msgstr ""
"Аргумент змінної Event в %L має бути комасивом, але не коіндексованим"

#: fortran/resolve.c:9162 fortran/resolve.c:9283
#, gcc-internal-format, gfc-internal-format
msgid "STAT= argument at %L must be a scalar INTEGER variable"
msgstr "Аргумент STAT= в %L має бути скалярною змінною INTEGER"

#: fortran/resolve.c:9174 fortran/resolve.c:9290
#, gcc-internal-format, gfc-internal-format
msgid "ERRMSG= argument at %L must be a scalar CHARACTER variable"
msgstr "Аргумент ERRMSG= в %L має бути скалярною змінною CHARACTER"

#: fortran/resolve.c:9186
#, gcc-internal-format, gfc-internal-format
msgid "ACQUIRED_LOCK= argument at %L must be a scalar LOGICAL variable"
msgstr "Аргумент ACQUIRED_LOCK=  в %L має бути скалярною змінною LOGICAL"

#: fortran/resolve.c:9199
#, gcc-internal-format, gfc-internal-format
msgid "UNTIL_COUNT= argument at %L must be a scalar INTEGER expression"
msgstr "Аргумент UNTIL_COUNT= в %L має бути скалярним виразом INTEGER"

#: fortran/resolve.c:9260
#, gcc-internal-format, gfc-internal-format
msgid "Imageset argument at %L must be a scalar or rank-1 INTEGER expression"
msgstr ""
"Аргумент Imageset в %L має бути скалярним або рангу 1 виразом INTEGER"

#: fortran/resolve.c:9264 fortran/resolve.c:9274
#, gcc-internal-format, gfc-internal-format
msgid "Imageset argument at %L must between 1 and num_images()"
msgstr "Аргумент Imageset в %L має бути між 1 та num_images()"

#: fortran/resolve.c:9317
#, gcc-internal-format, gfc-internal-format
msgid ""
"Statement at %L is not a valid branch target statement for the branch "
"statement at %L"
msgstr ""
"Оператор в %L не є припустимим оператором цілі переходу для оператора "
"переходу в %L"

#: fortran/resolve.c:9327
#, gcc-internal-format, gfc-internal-format
msgid "Branch at %L may result in an infinite loop"
msgstr "Перехід в %L може призвести до зациклювання"

#. Note: A label at END CRITICAL does not leave the CRITICAL
#. construct as END CRITICAL is still part of it.
#: fortran/resolve.c:9344 fortran/resolve.c:9367
#, gcc-internal-format, gfc-internal-format
msgid "GOTO statement at %L leaves CRITICAL construct for label at %L"
msgstr "Оператор GOTO в %L полишає конструкцію CRITICAL для мітки в %L"

#: fortran/resolve.c:9348 fortran/resolve.c:9373
#, gcc-internal-format, gfc-internal-format
msgid "GOTO statement at %L leaves DO CONCURRENT construct for label at %L"
msgstr "Оператор GOTO в %L полишає конструкцію DO CONCURRENT для мітки в %L"

#. The label is not in an enclosing block, so illegal.  This was
#. allowed in Fortran 66, so we allow it as extension.  No
#. further checks are necessary in this case.
#: fortran/resolve.c:9388
#, gcc-internal-format, gfc-internal-format
msgid "Label at %L is not in the same block as the GOTO statement at %L"
msgstr ""
"Мітка в %L не знаходиться в тому самому блоці, що і оператор GOTO в %L"

#: fortran/resolve.c:9460
#, gcc-internal-format, gfc-internal-format
msgid "WHERE mask at %L has inconsistent shape"
msgstr "Маска WHERE в %L имеет суперечливу форму"

#: fortran/resolve.c:9476
#, gcc-internal-format, gfc-internal-format
msgid "WHERE assignment target at %L has inconsistent shape"
msgstr "Мета привласнення WHERE в %L має суперечливу форму"

#: fortran/resolve.c:9484 fortran/resolve.c:9571
#, gcc-internal-format, gfc-internal-format
msgid "Non-ELEMENTAL user-defined assignment in WHERE at %L"
msgstr "Визначене користувачем привласнення Non-ELEMENTAL у WHERE в %L"

#: fortran/resolve.c:9494 fortran/resolve.c:9581
#, gcc-internal-format, gfc-internal-format
msgid "Unsupported statement inside WHERE at %L"
msgstr "Непідтримуваний оператор всередині WHERE в %L"

#: fortran/resolve.c:9525
#, gcc-internal-format, gfc-internal-format
msgid "Assignment to a FORALL index variable at %L"
msgstr "Привласнення індексної змінної FORALL в %L"

#: fortran/resolve.c:9534
#, gcc-internal-format
msgid ""
"The FORALL with index %qs is not used on the left side of the assignment at "
"%L and so might cause multiple assignment to this object"
msgstr ""
"FORALL з індексом %qs не використовується в лівій частині привласнення в %L "
"і тому може викликати множинне привласнення цього об'єкту"

#: fortran/resolve.c:9699
#, gcc-internal-format, gfc-internal-format
msgid "FORALL index-name at %L must be a scalar variable of type integer"
msgstr ""
"Індекс-ім'я FORALL в %L має бути скалярною змінною цілочисельного типу"

#: fortran/resolve.c:9709
#, gcc-internal-format, gfc-internal-format
msgid "An outer FORALL construct already has an index with this name %L"
msgstr "Зовнішня конструкція FORALL вже має індекс з цим іменем %L"

#: fortran/resolve.c:9786
#, gcc-internal-format, gfc-internal-format
msgid "WHERE/ELSEWHERE clause at %L requires a LOGICAL array"
msgstr "Речення WHERE/ELSEWHERE в %L потребує масив LOGICAL"

#: fortran/resolve.c:9888
#, gcc-internal-format
msgid "gfc_resolve_blocks(): Bad block type"
msgstr "gfc_resolve_blocks(): Помилковий тип блоку"

#: fortran/resolve.c:10002
#, gcc-internal-format, gfc-internal-format
msgid "CHARACTER expression will be truncated in assignment (%d/%d) at %L"
msgstr "Вираз CHARACTER буде скорочений в привласненні (%d/%d) в %L"

#: fortran/resolve.c:10034
#, gcc-internal-format, gfc-internal-format
msgid ""
"Coindexed expression at %L is assigned to a derived type variable with a "
"POINTER component in a PURE procedure"
msgstr ""
"Коіндексований вираз в %L привласнюється до змінної похідного типу з "
"компонентом POINTER в процедурі PURE"

#: fortran/resolve.c:10039
#, gcc-internal-format, gfc-internal-format
msgid ""
"The impure variable at %L is assigned to a derived type variable with a "
"POINTER component in a PURE procedure (12.6)"
msgstr ""
"Змінна Impure в %L привласнюється до змінної похідного типу з компонентом "
"POINTER в процедурі PURE (12.6)"

#: fortran/resolve.c:10049
#, gcc-internal-format, gfc-internal-format
msgid "Assignment to coindexed variable at %L in a PURE procedure"
msgstr "Привласнення коіндексованої змінної в %L в процедурі PURE"

#: fortran/resolve.c:10081
#, gcc-internal-format, gfc-internal-format
msgid "Assignment to polymorphic coarray at %L is not permitted"
msgstr "Привласнення поліморфного комасиву в %L недозволене"

#: fortran/resolve.c:10085
#, gcc-internal-format, gfc-internal-format
msgid "Assignment to an allocatable polymorphic variable at %L"
msgstr "Привласнення розміщуваної поліморфної змінної в %L"

#: fortran/resolve.c:10090
#, gcc-internal-format
msgid ""
"Assignment to an allocatable polymorphic variable at %L requires %<-frealloc-"
"lhs%>"
msgstr ""
"Привласнення розміщуваної поліморфної змінної в %L потребує %<-frealloc-lhs%>"

#: fortran/resolve.c:10097
#, gcc-internal-format, gfc-internal-format
msgid ""
"Nonallocatable variable must not be polymorphic in intrinsic assignment at "
"%L - check that there is a matching specific subroutine for '=' operator"
msgstr ""
"Нерозміщувана змінна не повинна бути поліморфною в внутрішньому привласненні "
"в %L - перевірте, що є відповідна конкретна підпрограма для оператора '='"

#: fortran/resolve.c:10108
#, gcc-internal-format, gfc-internal-format
msgid ""
"Coindexed variable must not have an allocatable ultimate component in "
"assignment at %L"
msgstr ""
"Коіндексована змінна не повинна мати розміщуваних остаточних компонентів в "
"привласненні в %L"

#: fortran/resolve.c:10413
#, gcc-internal-format, gfc-internal-format
msgid ""
"TODO: type-bound defined assignment(s) at %L not done because multiple part "
"array references would occur in intermediate expressions."
msgstr ""
"TODO: type-bound визначені привласненні в %L не зроблено, тому що в "
"проміжних виразах будуть траплятися посилання багаточастинних масивів"

#. Even if standard does not support this feature, continue to build
#. the two statements to avoid upsetting frontend_passes.c.
#: fortran/resolve.c:10655
#, gcc-internal-format, gfc-internal-format
msgid "Pointer procedure assignment at %L"
msgstr "Привласнення вказівника на процедуру %L"

#: fortran/resolve.c:10667
#, gcc-internal-format, gfc-internal-format
msgid ""
"The function result on the lhs of the assignment at %L must have the pointer "
"attribute."
msgstr ""
"Результат функції у лівій частині привласнення в %L повинен мати вказівний "
"атрибут"

#: fortran/resolve.c:10903
#, gcc-internal-format, gfc-internal-format
msgid "ASSIGNED GOTO statement at %L requires an INTEGER variable"
msgstr "Оператор ASSIGNED GOTO в %L потребує змінної INTEGER"

#: fortran/resolve.c:11894
#, gcc-internal-format
msgid "Variable %qs has not been assigned a target label at %L"
msgstr "Змінна %qs не була привласнена до цільової мітки %L"

#: fortran/resolve.c:10917
#, gcc-internal-format, gfc-internal-format
msgid ""
"Alternate RETURN statement at %L requires a SCALAR-INTEGER return specifier"
msgstr ""
"Альтернативний оператор RETURN в %L потребує специфікатор повернення SCALAR-"
"INTEGER"

#: fortran/resolve.c:10979
#, gcc-internal-format, gfc-internal-format
msgid "ASSIGN statement at %L requires a scalar default INTEGER variable"
msgstr "Оператор ASSIGN в %L потребує скалярну типову змінну INTEGER"

#: fortran/resolve.c:11027
#, gcc-internal-format, gfc-internal-format
msgid "Invalid NULL at %L"
msgstr "Неприпустимий NULL в %L"

#: fortran/resolve.c:11031
#, gcc-internal-format, gfc-internal-format
msgid ""
"Arithmetic IF statement at %L requires a scalar REAL or INTEGER expression"
msgstr ""
"Оператор арифметичного IF в %L потребує скалярного виразу REAL або INTEGER"

#: fortran/resolve.c:12089
#, gcc-internal-format
msgid "gfc_resolve_code(): No expression on DO WHILE"
msgstr "gfc_resolve_code(): Немає виразу в DO WHILE"

#: fortran/resolve.c:11092
#, gcc-internal-format, gfc-internal-format
msgid ""
"Exit condition of DO WHILE loop at %L must be a scalar LOGICAL expression"
msgstr ""
"Умова виходу з циклу DO WHILE в %L має бути скалярний виразом LOGICAL"

#: fortran/resolve.c:11176
#, gcc-internal-format, gfc-internal-format
msgid "FORALL mask clause at %L requires a scalar LOGICAL expression"
msgstr "Речення маски FORALL в %L потребує скалярний вираз LOGICAL"

#: fortran/resolve.c:12260
#, gcc-internal-format
msgid "gfc_resolve_code(): Bad statement code"
msgstr "gfc_resolve_code(): Помилковий код оператора"

#: fortran/resolve.c:11365
#, gcc-internal-format, gfc-internal-format
msgid ""
"Variable %s with binding label %s at %L uses the same global identifier as "
"entity at %L"
msgstr ""
"Змінна %s із зв'язувальною міткою %s в %L використовує такий самий "
"глобальний ідентифікатор, як і сутність в %L"

#. This can only happen if the variable is defined in a module - if it
#. isn't the same module, reject it.
#: fortran/resolve.c:11378
#, gcc-internal-format, gfc-internal-format
msgid ""
"Variable %s from module %s with binding label %s at %L uses the same global "
"identifier as entity at %L from module %s"
msgstr ""
"Змінна %s з модуля %s  із зв'язувальною міткою %s в %L використовує такий "
"самий глобальний ідентифікатор, як і сутність в %L з модуля %s"

#. Print an error if the procedure is defined multiple times; we have to
#. exclude references to the same procedure via module association or
#. multiple checks for the same procedure.
#: fortran/resolve.c:11395
#, gcc-internal-format, gfc-internal-format
msgid ""
"Procedure %s with binding label %s at %L uses the same global identifier as "
"entity at %L"
msgstr ""
"Процедура %s із зв'язувальною міткою %s в %L використовує такий самий "
"глобальний ідентифікатор, як і сутність в %L"

#: fortran/resolve.c:11472
#, gcc-internal-format, gfc-internal-format
msgid "String length at %L is too large"
msgstr "Довжина рядка в %L завелика"

#: fortran/resolve.c:11701
#, gcc-internal-format
msgid ""
"Allocatable array %qs at %L must have a deferred shape or assumed rank"
msgstr ""
"Розміщуваний масив %qs в %L повинен мати відкладену форму або передбачуваний "
"ранг"

#: fortran/resolve.c:12732
#, gcc-internal-format
msgid "Scalar object %qs at %L may not be ALLOCATABLE"
msgstr "Скалярний об'єкт %qs в %L не може бути ALLOCATABLE"

#: fortran/resolve.c:12740
#, gcc-internal-format
msgid "Array pointer %qs at %L must have a deferred shape or assumed rank"
msgstr ""
"Вказівник на масив %qs в %L повинен мати відкладену форму або передбачуваний "
"ранг"

#: fortran/resolve.c:12751
#, gcc-internal-format
