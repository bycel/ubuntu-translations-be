msgstr "Змінна %qs не може з'являтися у виразі в %L"

#: fortran/expr.c:3015
#, gcc-internal-format
msgid "check_restricted(): Unknown expression type"
msgstr "check_restricted(): Невідомий тип виразу"

#: fortran/expr.c:3035
#, gcc-internal-format, gfc-internal-format
msgid "Expression at %L must be of INTEGER type, found %s"
msgstr "Вираз в %L має бути типом INTEGER, знайдено %s"

#: fortran/expr.c:3047
#, gcc-internal-format
msgid "Function %qs at %L must be PURE"
msgstr "Функція %qs в %L має бути PURE"

#: fortran/expr.c:3056
#, gcc-internal-format, gfc-internal-format
msgid "Expression at %L must be scalar"
msgstr "Вираз в %L має бути скалярним"

#: fortran/expr.c:3090
#, gcc-internal-format, gfc-internal-format
