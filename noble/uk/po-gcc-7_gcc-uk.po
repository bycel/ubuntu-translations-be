#: fortran/expr.c:5348
#, gcc-internal-format
msgid ""
"Dummy argument %qs with INTENT(IN) in variable definition context (%s) at %L"
msgstr ""
"Фіктивний аргумент %qs з INTENT(IN) в контексті визначення змінних (%s) в %L"

#: fortran/expr.c:5361
#, gcc-internal-format
msgid ""
"Variable %qs is PROTECTED and can not appear in a pointer association "
"context (%s) at %L"
msgstr ""
"Змінна %qs є PROTECTED і не може з'являтися в контексті асоціації "
"вказівників (%s) в %L"

#: fortran/expr.c:5369
#, gcc-internal-format
msgid ""
"Variable %qs is PROTECTED and can not appear in a variable definition "
"context (%s) at %L"
msgstr ""
"Змінна %qs є PROTECTED і не може з'являтися в контексті визначення змінних "
"(%s) в %L"

#: fortran/expr.c:5381
#, gcc-internal-format
msgid ""
"Variable %qs can not appear in a variable definition context (%s) at %L in "
"PURE procedure"
msgstr ""
"Змінна %qs не можуть з'являтися в контексті визначення змінних (%s) в %L у "
"процедурі PURE"

#: fortran/expr.c:5440
#, gcc-internal-format
msgid ""
"%qs at %L associated to vector-indexed target can not be used in a variable "
"definition context (%s)"
msgstr ""
"%qs в %L асоційований з векторно-індексованою ціллюі не може "
"використовуватись в контексті визначення змінних (%s)"

#: fortran/expr.c:5444
#, gcc-internal-format
msgid ""
"%qs at %L associated to expression can not be used in a variable definition "
"context (%s)"
msgstr ""
"%qs в %L асоційований з виразом не може використовуватись в контексті "
"визначення змінних (%s)"

#: fortran/expr.c:5455
#, gcc-internal-format
msgid ""
"Associate-name %qs can not appear in a variable definition context (%s) at "
"%L because its target at %L can not, either"
msgstr ""
"Асоціативна назва %qs не може з'являтися в контексті визначення змінних (%s) "
"в %L, оскільки її ціль в %L не може"

#: fortran/expr.c:5497
#, gcc-internal-format, gfc-internal-format
msgid ""
"Elements with the same value at %L and %L in vector subscript in a variable "
"definition context (%s)"
msgstr ""
"Елементи з тим самим значенням в %L та %L у векторному індексі в контексті "
"визначенні змінної (%s)"

#: fortran/f95-lang.c:251
#, gcc-internal-format, gfc-internal-format
msgid "can't open input file: %s"
msgstr "не вдалося відкрити вхідний файл %s"

#: fortran/frontend-passes.c:167
#, gcc-internal-format
msgid "No location in statement"
msgstr "Не вказано місця у операторі"

#: fortran/frontend-passes.c:182
#, gcc-internal-format, gfc-internal-format
msgid "No location in expression near %L"
msgstr "Не вказано місця у виразі поряд із %L"

#: fortran/frontend-passes.c:381
#, gcc-internal-format
msgid "Illegal id in copy_walk_reduction_arg"
msgstr "Невірний id в copy_walk_reduction_arg"

#: fortran/frontend-passes.c:781 fortran/trans-array.c:1044
#: fortran/trans-array.c:6232 fortran/trans-array.c:7633
#: fortran/trans-intrinsic.c:7067
#, gcc-internal-format, gfc-internal-format
msgid "Creating array temporary at %L"
msgstr "Створення тимчасового масиву в %L"

#: fortran/frontend-passes.c:806 fortran/frontend-passes.c:810
#, gcc-internal-format
msgid "Removing call to function %qs at %L"
msgstr "Вилучення виклику функції %qs в %L"

#: fortran/frontend-passes.c:1797
#, gcc-internal-format
msgid "illegal OP in optimize_comparison"
msgstr "некоректний оператор в optimize_comparison"

#: fortran/frontend-passes.c:1989
#, gcc-internal-format
msgid ""
"Variable %qs at %L set to undefined value inside loop  beginning at %L as "
"INTENT(OUT) argument to subroutine %qs"
msgstr ""
"Змінна %qs в %L встановлена у невизначене значення всередині циклу, що "
"починається в %L як аргумент INTENT(OUT) до підпроцедури %qs"

#: fortran/frontend-passes.c:1996
#, gcc-internal-format
msgid ""
"Variable %qs at %L not definable inside loop beginning at %L as "
"INTENT(INOUT) argument to subroutine %qs"
msgstr ""
"Змінна %qs в %L недоступна для визначення всередині циклу, що починається в "
"%L як аргумент INTENT(OUT) до підпроцедури %qs"

#: fortran/frontend-passes.c:2061
#, gcc-internal-format
msgid ""
"Variable %qs at %L set to undefined value inside loop beginning at %L as "
"INTENT(OUT) argument to function %qs"
msgstr ""
"Змінна %qs в %L встановлена у невизначене значення всередині циклу, що "
"починається в %L як аргумент INTENT(OUT) до фукції %qs"

#: fortran/frontend-passes.c:2067
#, gcc-internal-format
msgid ""
"Variable %qs at %L not definable inside loop beginning at %L as "
"INTENT(INOUT) argument to function %qs"
msgstr ""
"Змінна %qs в %L недоступна для визначення всередині циклу, що починається в "
"%L як аргумент INTENT(OUT) до функції %qs"

#: fortran/frontend-passes.c:2315 fortran/trans-expr.c:1526
#, gcc-internal-format, gfc-internal-format
msgid "Code for reallocating the allocatable array at %L will be added"
msgstr "Код для перерозміщення розміщуваного масиву в %L буде доданий"

#: fortran/frontend-passes.c:2794
#, gcc-internal-format
msgid "Scalarization using DIMEN_RANGE unimplemented"
msgstr "Скалярізація за допомогою DIMEN_RANGE не реалізована"

#: fortran/gfortranspec.c:145
#, gcc-internal-format
msgid "overflowed output arg list for %qs"
msgstr "переповнення списку вихідних аргументів для %qs"

#: fortran/gfortranspec.c:299
#, gcc-internal-format
msgid "no input files; unwilling to write output files"
msgstr "немає вхідних файлів; немає можливості записати вихідні файли"

#: fortran/interface.c:225
#, gcc-internal-format, gfc-internal-format
msgid "Syntax error in generic specification at %C"
msgstr "Синтаксична помилка в generic-специфікації в %C"

#: fortran/interface.c:252
#, gcc-internal-format, gfc-internal-format
msgid "Syntax error: Trailing garbage in INTERFACE statement at %C"
msgstr "Синтаксична помилка: Сміття в кінці оператора INTERFACE в %C"

#: fortran/interface.c:272
#, gcc-internal-format
msgid "Dummy procedure %qs at %C cannot have a generic interface"
msgstr "Формальна процедура %qs в %C не може мати generic-інтерфейс"

#: fortran/interface.c:305
#, gcc-internal-format, gfc-internal-format
msgid "ABSTRACT INTERFACE at %C"
msgstr "ABSTRACT INTERFACE в %C"

#: fortran/interface.c:312
#, gcc-internal-format, gfc-internal-format
msgid "Syntax error in ABSTRACT INTERFACE statement at %C"
msgstr "Синтаксична помилка в операторі ABSTRACT INTERFACE в %C"

#: fortran/interface.c:343
#, gcc-internal-format, gfc-internal-format
msgid "Syntax error: Trailing garbage in END INTERFACE statement at %C"
msgstr "Синтаксична помилка: Сміття в кінці оператора END INTERFACE в %C"

#: fortran/interface.c:356
#, gcc-internal-format, gfc-internal-format
msgid "Expected a nameless interface at %C"
msgstr "Мав бути безіменний інтерфейс в %C"

#: fortran/interface.c:369
#, gcc-internal-format
msgid "Expected %<END INTERFACE ASSIGNMENT (=)%> at %C"
msgstr "Мало бути %<END INTERFACE ASSIGNMENT (=)%> в %C"

#: fortran/interface.c:400
#, gcc-internal-format
msgid "Expecting %<END INTERFACE OPERATOR (%s)%> at %C"
msgstr "Мало бути %<END INTERFACE OPERATOR (%s)%> в %C"

#: fortran/interface.c:403
#, gcc-internal-format
msgid "Expecting %<END INTERFACE OPERATOR (%s)%> at %C, but got %qs"
msgstr "Мало бути %<END INTERFACE OPERATOR (%s)%> в %C, а отримано %qs"

#: fortran/interface.c:417
#, gcc-internal-format
msgid "Expecting %<END INTERFACE OPERATOR (.%s.)%> at %C"
msgstr "Мало бути %<END INTERFACE OPERATOR (.%s.)%> в %C"

#: fortran/interface.c:429
#, gcc-internal-format
msgid "Expecting %<END INTERFACE %s%> at %C"
msgstr "Мало бути %<END INTERFACE %s%> в %C"

#: fortran/interface.c:619
#, gcc-internal-format
msgid "gfc_compare_derived_types: invalid derived type"
msgstr "gfc_compare_derived_types: неприпустимий похідний тип"

#: fortran/interface.c:866
#, gcc-internal-format, gfc-internal-format
msgid "Alternate return cannot appear in operator interface at %L"
msgstr ""
"Альтернативний результат не може з'являтися в інтерфейсі оператора в %L"

#: fortran/interface.c:895
#, gcc-internal-format, gfc-internal-format
msgid "Assignment operator interface at %L must have two arguments"
msgstr "Інтерфейс оператора привласнення в %L повинен мати два аргументи"

#: fortran/interface.c:898
#, gcc-internal-format, gfc-internal-format
msgid "Operator interface at %L has the wrong number of arguments"
msgstr "Інтерфейс оператора в %L має невірне число аргументів"

#: fortran/interface.c:911
#, gcc-internal-format, gfc-internal-format
msgid "Assignment operator interface at %L must be a SUBROUTINE"
msgstr "Інтерфейс оператора привласнення в %L має бути SUBROUTINE"

#: fortran/interface.c:929
#, gcc-internal-format, gfc-internal-format
msgid ""
"Assignment operator interface at %L must not redefine an INTRINSIC type "
"assignment"
msgstr ""
"Інтерфейс оператора привласнення в %L не повинен перевизначати привласнення "
"типу INTRINSIC"

#: fortran/interface.c:938
#, gcc-internal-format, gfc-internal-format
msgid "Intrinsic operator interface at %L must be a FUNCTION"
msgstr "Інтерфейс внутрішнього оператора в %L має бути FUNCTION"

#: fortran/interface.c:949
#, gcc-internal-format, gfc-internal-format
msgid ""
"First argument of defined assignment at %L must be INTENT(OUT) or "
"INTENT(INOUT)"
msgstr ""
"Перший аргумент визначеного привласнення в %L має бути INTENT(OUT) або "
"INTENT(INOUT)"

#: fortran/interface.c:956
#, gcc-internal-format, gfc-internal-format
msgid "Second argument of defined assignment at %L must be INTENT(IN)"
msgstr "Другий аргумент визначеного привласнення в %L має бути INTENT(IN)"

#: fortran/interface.c:965 fortran/resolve.c:15882
#, gcc-internal-format, gfc-internal-format
msgid "First argument of operator interface at %L must be INTENT(IN)"
msgstr "Перший аргумент інтерфейсу оператора в %L має бути INTENT(IN)"

#: fortran/interface.c:972 fortran/resolve.c:15900
#, gcc-internal-format, gfc-internal-format
msgid "Second argument of operator interface at %L must be INTENT(IN)"
msgstr "Другий аргумент інтерфейсу оператора в %L має бути INTENT(IN)"

#: fortran/interface.c:1077
#, gcc-internal-format, gfc-internal-format
msgid "Operator interface at %L conflicts with intrinsic interface"
msgstr "Інтерфейс оператора в %L суперечить вбудованому інтерфейсу"

#: fortran/interface.c:1409 fortran/interface.c:1468
#, gcc-internal-format, gfc-internal-format
msgid ""
"check_dummy_characteristics: Unexpected result %i of gfc_dep_compare_expr"
msgstr ""
"check_dummy_characteristics: Неочікуванй результат %i для "
"gfc_dep_compare_expr"

#: fortran/interface.c:1581
#, gcc-internal-format, gfc-internal-format
msgid ""
"check_result_characteristics (1): Unexpected result %i of "
"gfc_dep_compare_expr"
msgstr ""
"check_result_characteristics (1): Неочікуванй результат %i для "
"gfc_dep_compare_expr"

#: fortran/interface.c:1628
#, gcc-internal-format, gfc-internal-format
msgid ""
"check_result_characteristics (2): Unexpected result %i of "
"gfc_dep_compare_expr"
msgstr ""
"check_result_characteristics (2): Неочікуваний результат %i для "
"gfc_dep_compare_expr"

#: fortran/interface.c:1883
#, gcc-internal-format
msgid "Procedure %qs in %s at %L has no explicit interface"
msgstr "Процедура %qs у %s в %L не має явного інтерфейсу"

#: fortran/interface.c:1891
#, gcc-internal-format
msgid "Procedure %qs in %s at %L is neither function nor subroutine"
msgstr "Процедура %qs у %s в %L не є ані функцією, ані підпроцедурою"

#: fortran/interface.c:1811
#, gcc-internal-format, gfc-internal-format
msgid ""
"In %s at %L procedures must be either all SUBROUTINEs or all FUNCTIONs"
msgstr "В %s в %L процедури мают бути всі або SUBROUTINE, або FUNCTION"

#: fortran/interface.c:1815
#, gcc-internal-format, gfc-internal-format
msgid ""
"In %s at %L procedures must be all FUNCTIONs as the generic name is also the "
"name of a derived type"
msgstr ""
"В %s в %L процедури мают бути всі або FUNCTION, оскільки як generic-ім'я є "
"також іменем похідного типу"

#: fortran/interface.c:1915
#, gcc-internal-format
msgid "Internal procedure %qs in %s at %L"
msgstr "Внутрішня процедура %qs в %s у %L"

#: fortran/interface.c:1972 fortran/interface.c:1977
#, gcc-internal-format
msgid "Ambiguous interfaces in %s for %qs at %L and %qs at %L"
msgstr "Неоднозначні інтерйфейси в %s для %qs в %L та %qs в %L"

#: fortran/interface.c:1982
#, gcc-internal-format
msgid "Although not referenced, %qs has ambiguous interfaces at %L"
msgstr "Хоча не за посиланням, %qs має неоднозначні інтерфейси в %L"

#: fortran/interface.c:2020
#, gcc-internal-format
msgid "%qs at %L is not a module procedure"
msgstr "%qs у %L не є процедурою модуля"

#: fortran/interface.c:2128
#, gcc-internal-format
msgid ""
"The assumed-rank array at %L requires that the dummy argument %qs has "
"assumed-rank"
msgstr ""
"Масив передбачуваної форми в %L потребує, щоб формальний аргумент %qs мав "
"передбачуваний ранг"

#: fortran/interface.c:2228
#, gcc-internal-format
msgid "Rank mismatch in argument %qs at %L (scalar and rank-%d)"
msgstr "Невідповідність рангу в аргументі %qs в %L (скаляр та ранг-%d)"

#: fortran/interface.c:2134
#, gcc-internal-format
msgid "Rank mismatch in argument %qs at %L (rank-%d and scalar)"
msgstr "Невідповідність рангу в аргументі %qs в %L (ранг-%d та скаляр)"

#: fortran/interface.c:2137
#, gcc-internal-format
msgid "Rank mismatch in argument %qs at %L (rank-%d and rank-%d)"
msgstr "Невідповідність рангу в аргументі %qs в %L (ранг-%d та ранг-%d)"

#: fortran/interface.c:2179
#, gcc-internal-format, gfc-internal-format
msgid "Invalid procedure argument at %L"
msgstr "Неприпустимий аргумент процедури в %L"

#: fortran/interface.c:2188 fortran/interface.c:2216
#, gcc-internal-format
msgid "Interface mismatch in dummy procedure %qs at %L: %s"
msgstr "Невідповідність інтерфейсу в формальній процедурі %qs в %L: %s"

#: fortran/interface.c:2227
#, gcc-internal-format
msgid ""
"Actual argument to contiguous pointer dummy %qs at %L must be simply "
"contiguous"
msgstr ""
"Фактичний аргумент для неперервного вказівного формального аргументу  %qs в "
"%L має бути просто неперервним"

#: fortran/interface.c:2247
#, gcc-internal-format
msgid "Type mismatch in argument %qs at %L; passed %s to %s"
msgstr "Невідповідність типу в аргументі %qs у %L; передано %s до %s"

#: fortran/interface.c:2256
#, gcc-internal-format
msgid ""
"Assumed-type actual argument at %L requires that dummy argument %qs is of "
"assumed type"
msgstr ""
"Фактичний аргумент передбачуваної форми в %L потребує, щоб формальний "
"аргумент %qs був передбачуваного типу"

#: fortran/interface.c:2272
#, gcc-internal-format
msgid "Actual argument to %qs at %L must be polymorphic"
msgstr "Фактичний аргумент до %qs в %L має бути поліморфним"

#: fortran/interface.c:2282
#, gcc-internal-format
msgid "Actual argument to %qs at %L must have the same declared type"
msgstr ""
"Фактичний аргумент до %qs в %L повинен мати той самий декларований тип"

#: fortran/interface.c:2297
#, gcc-internal-format
msgid ""
"Actual argument to %qs at %L must be unlimited polymorphic since the formal "
"argument is a pointer or allocatable unlimited polymorphic entity [F2008: "
"12.5.2.5]"
msgstr ""
"Фактичний аргумент до %qs в %L має бути безлімітним поліморфним оскільки "
"формальний аргумент є вказівником або доступною для розміщення безлімітною "
"поліморфною сутністю [F2008: 12.5.2.5]"

#: fortran/interface.c:2308
#, gcc-internal-format
msgid "Actual argument to %qs at %L must be a coarray"
msgstr "Фактичний аргумент до %qs в %L повинен бути комасивом"

#: fortran/interface.c:2327
#, gcc-internal-format
msgid "Corank mismatch in argument %qs at %L (%d and %d)"
msgstr "Невідповідність корангів в аргументі %qs в %L (%d та %d)"

#: fortran/interface.c:2345
#, gcc-internal-format
msgid ""
"Actual argument to %qs at %L must be simply contiguous or an element of such "
"an array"
msgstr ""
"Фактичний аргумент для %s в %L має бути просто неперервним або елементом "
"такого масиву"

#: fortran/interface.c:2360
#, gcc-internal-format
msgid ""
"Actual argument to non-INTENT(INOUT) dummy %qs at %L, which is LOCK_TYPE or "
"has a LOCK_TYPE component"
msgstr ""
"Фактичний аргумент для non-INTENT(INOUT) формального аргументу %qs в %L, "
"котрий є LOCK_TYPE або має компонент LOCK_TYPE"

#: fortran/interface.c:2569
#, gcc-internal-format
msgid ""
"Actual argument to non-INTENT(INOUT) dummy %qs at %L, which is EVENT_TYPE or "
"has a EVENT_TYPE component"
msgstr ""
"Фактичний аргумент для non-INTENT(INOUT) формального аргументу %qs в %L, "
"котрий є EVENT_TYPE або має компонент EVENT_TYPE"

#: fortran/interface.c:2588
#, gcc-internal-format
msgid ""
"Dummy argument %qs has to be a pointer, assumed-shape or assumed-rank array "
"without CONTIGUOUS attribute - as actual argument at %L is not simply "
"contiguous and both are ASYNCHRONOUS or VOLATILE"
msgstr ""
"Формальний аргумент %qs має бути вказівником чи масивом передбачуваної форми "
"без атрибута CONTIGUOUS - оскільки фактичний аргумент в %L не є просто "
"неперервним і обидва є ASYNCHRONOUS або VOLATILE"

#: fortran/interface.c:2601
#, gcc-internal-format
msgid ""
"Passing coarray at %L to allocatable, noncoarray, INTENT(OUT) dummy argument "
"%qs"
msgstr ""
"Передавання комасиву в %L до доступного для розміщення некомасивного "
"INTENT(OUT) формального аргументу %qs"

#: fortran/interface.c:2608
#, gcc-internal-format
msgid ""
"Passing coarray at %L to allocatable, noncoarray dummy argument %qs, which "
"is invalid if the allocation status is modified"
msgstr ""
"Передавання комасиву в %L в доступному для розміщення некомасивного "
"формальному аргументі %s, який є неприпустимий, якщо модифікований статус "
"розміщення"

#: fortran/interface.c:2682
#, gcc-internal-format
msgid "Polymorphic scalar passed to array dummy argument %qs at %L"
msgstr "Поліморфний скаляр переданий в формальний аргумент-масив %qs в %L"

#: fortran/interface.c:2486
#, gcc-internal-format
msgid ""
"Element of assumed-shaped or pointer array passed to array dummy argument "
"%qs at %L"
msgstr ""
"Елемент масиву передбачуваної форми або вказівного масиву переданий до "
"формальногой аргумент-масиву %qs в %L"

#: fortran/interface.c:2711
#, gcc-internal-format
msgid ""
"Extension: Scalar non-default-kind, non-C_CHAR-kind CHARACTER actual "
"argument with array dummy argument %qs at %L"
msgstr ""
"Розширення: Скалярний нетиповий вид, не-C_CHAR-виду CHARACTER фактичний "
"аргумент з формальним аргумент-масивом %qs at %L"

#: fortran/interface.c:2719
#, gcc-internal-format
msgid ""
"Fortran 2003: Scalar CHARACTER actual argument with array dummy argument %qs "
"at %L"
msgstr ""
"Fortran 2003: Скалярний CHARACTER фактичний аргумент з формальним аргумент-"
"масивом %s в %L"

#: fortran/interface.c:3097
#, gcc-internal-format
msgid "Keyword argument %qs at %L is not in the procedure"
msgstr "Аргумент з ключовим словом %qs в %L не в процедурі"

#: fortran/interface.c:3106
#, gcc-internal-format
msgid ""
"Keyword argument %qs at %L is already associated with another actual argument"
msgstr ""
"Аргумент з ключовим словом %qs в %LL вже асоційований з іншим фактичним "
"аргументом"

#: fortran/interface.c:2851
#, gcc-internal-format, gfc-internal-format
msgid "More actual than formal arguments in procedure call at %L"
msgstr "Більше фактичних ніж формальних аргументів у виклику процедури в %L"

#: fortran/interface.c:2863 fortran/interface.c:3243
#, gcc-internal-format, gfc-internal-format
msgid "Missing alternate return spec in subroutine call at %L"
msgstr ""
"Відсутня специфікація альтернативного повернення у виклику підпроцедури в %L"

#: fortran/interface.c:2871
#, gcc-internal-format, gfc-internal-format
msgid "Unexpected alternate return spec in subroutine call at %L"
msgstr ""
"Відсутня специфікація альтернативного повернення у виклику підпроцедури в %L"

#: fortran/interface.c:3171
#, gcc-internal-format
msgid "Unexpected NULL() intrinsic at %L to dummy %qs"
msgstr "Неочікуване NULL() всередині у %L до фіктивного %qs"

#: fortran/interface.c:3174
#, gcc-internal-format
msgid "Fortran 2008: Null pointer at %L to non-pointer dummy %qs"
msgstr ""
"Fortran 2008: Порожній вказівник %L не невказівний формальний параметр %qs"

#: fortran/interface.c:2925
#, gcc-internal-format, gfc-internal-format
msgid ""
"Actual argument at %L to assumed-type dummy is of derived type with type-"
"bound or FINAL procedures"
msgstr ""
"Фактичний аргумент в %L до формального аргументу передбачуваного типу є "
"похідним типом з type-bound або FINAL процедурами"

#: fortran/interface.c:3220
#, gcc-internal-format
msgid ""
"Character length mismatch (%ld/%ld) between actual argument and pointer or "
"allocatable dummy argument %qs at %L"
msgstr ""
"Невідповідність символьної довжини (%ld/%ld) між фактичним аргументом та "
"вказівним або розміщуваним формальним аргументом %qs в %L"

#: fortran/interface.c:2956
#, gcc-internal-format
msgid ""
"Character length mismatch (%ld/%ld) between actual argument and assumed-"
"shape dummy argument %qs at %L"
msgstr ""
"Невідповідність символьної довжини (%ld/%ld) між фактичним аргументом та "
"вказівним або формальним аргументом передбачуваної форми %qs в %L"

#: fortran/interface.c:2970
#, gcc-internal-format
msgid ""
"Actual argument at %L to allocatable or pointer dummy argument %qs must have "
"a deferred length type parameter if and only if the dummy has one"
msgstr ""
"Фактичний аргумент в %L до розміщуваного або вказівного формального "
"аргументу %qs повинен мати параметр типу відкладеної довжини тоді і тільки "
"тоді, коли такий є у формального параметра"

#: fortran/interface.c:2988
#, gcc-internal-format
msgid ""
"Character length of actual argument shorter than of dummy argument %qs "
"(%lu/%lu) at %L"
msgstr ""
"Символьна довжина фактичного аргументу коротше ніж у формальному аргументі "
"%qs (%lu/%lu) в %L"

#: fortran/interface.c:2994
#, gcc-internal-format
msgid ""
"Actual argument contains too few elements for dummy argument %qs (%lu/%lu) "
"at %L"
msgstr ""
"Фактичний аргумент містить занадто мало елементів для формального аргументу "
"%qs (%lu/%lu) в %L"

#: fortran/interface.c:3013
#, gcc-internal-format
msgid "Expected a procedure pointer for argument %qs at %L"
msgstr "Мав бути вказівник на процедуру для аргументу %qs в %L"

#: fortran/interface.c:3029
#, gcc-internal-format
msgid "Expected a procedure for argument %qs at %L"
msgstr "Мала бути процедура для аргументу %qs в %L"

#: fortran/interface.c:3043
#, gcc-internal-format
msgid "Actual argument for %qs cannot be an assumed-size array at %L"
msgstr ""
"Фактичний аргумент для %qs не може бути масивом передбачуваного розміру в %L"

#: fortran/interface.c:3052
#, gcc-internal-format
msgid "Actual argument for %qs must be a pointer at %L"
msgstr "Фактичний аргумент для %qs має бути вказівником в %L"

#: fortran/interface.c:3062
#, gcc-internal-format
msgid "Fortran 2008: Non-pointer actual argument at %L to pointer dummy %qs"
msgstr ""
"Fortran 2008: Невказівний фактичний аргумент в %L до вказівного формального "
"аргументу %qs"

#: fortran/interface.c:3072
#, gcc-internal-format
msgid "Coindexed actual argument at %L to pointer dummy %qs"
msgstr ""
"Коіндексований фактичний аргумент в %L до вказівного формального аргументу "
"%qs"

#: fortran/interface.c:3085
#, gcc-internal-format
msgid ""
"Coindexed actual argument at %L to allocatable dummy %qs requires INTENT(IN)"
msgstr ""
"Коіндексований фактичний аргумент в %L до розміщуваного формального "
"аргументу %qs вимагає INTENT(IN)"

#: fortran/interface.c:3099
#, gcc-internal-format
msgid ""
"Coindexed ASYNCHRONOUS or VOLATILE actual argument at %L requires that dummy "
"%qs has neither ASYNCHRONOUS nor VOLATILE"
msgstr ""
"Коіндексований ASYNCHRONOUS або VOLATILE фактичний аргумент у %L вимагає, "
"щоб формальний параметр %qs не мав ані ASYNCHRONOUS, ані VOLATILE"

#: fortran/interface.c:3113
#, gcc-internal-format
msgid ""
"Coindexed actual argument at %L with allocatable ultimate component to dummy "
"%qs requires either VALUE or INTENT(IN)"
msgstr ""
"Коіндексований фактичний аргумент у %L з розміщуваним кінцевим компонентом "
"для формального параметра %qs вимагає або VALUE, або INTENT(IN)"

#: fortran/interface.c:3125
#, gcc-internal-format
msgid "Actual CLASS array argument for %qs must be a full array at %L"
msgstr ""
"Фактичний аргумент CLASS array для %qs повинен бути повним масивом у %L"

#: fortran/interface.c:3135
#, gcc-internal-format
msgid "Actual argument for %qs must be ALLOCATABLE at %L"
msgstr "Фактичний аргумент для %qs повинен бути ALLOCATABLE в %L"

#: fortran/interface.c:3164
#, gcc-internal-format
msgid ""
"Array-section actual argument with vector subscripts at %L is incompatible "
"with INTENT(OUT), INTENT(INOUT), VOLATILE or ASYNCHRONOUS attribute of the "
"dummy argument %qs"
msgstr ""
"Фактичний аргумент секції-масиву з векторними індексами в %L несумісний з "
"атрибутом INTENT (OUT), INTENT (INOUT), VOLATILE або ASYNCHRONOUS "
"формального аргументу %qs"

#: fortran/interface.c:3183
#, gcc-internal-format
msgid ""
"Assumed-shape actual argument at %L is incompatible with the non-assumed-"
"shape dummy argument %qs due to VOLATILE attribute"
msgstr ""
"Фактичний аргумент передбачуваної форми в %L несумісний з непередбачуваною "
"формою формального аргументу %qs через атрибут VOLATILE"

#: fortran/interface.c:3200
#, gcc-internal-format
msgid ""
"Array-section actual argument at %L is incompatible with the non-assumed-"
"shape dummy argument %qs due to VOLATILE attribute"
msgstr ""
"Фактичний аргумент секції-масиву в %L несумісний з непередбачуваною формою "
"формального аргументу %qs через атрибут VOLATILE"

#: fortran/interface.c:3220
#, gcc-internal-format
msgid ""
"Pointer-array actual argument at %L requires an assumed-shape or pointer-"
"array dummy argument %qs due to VOLATILE attribute"
msgstr ""
"Фактичний аргумент вказівник-масиву в %L потребує з формального аргументу "
"передбачуваної форми або вказівник-масиву %qs через атрибут VOLATILE"

#: fortran/interface.c:3250
#, gcc-internal-format
msgid "Missing actual argument for argument %qs at %L"
msgstr "Відсутній фактичний аргумент для аргументу %qs в %L"

#: fortran/interface.c:3367
#, gcc-internal-format
msgid "compare_actual_expr(): Bad component code"
msgstr "compare_actual_expr(): Невірний код компонента"

#: fortran/interface.c:3396
#, gcc-internal-format
msgid "check_some_aliasing(): List mismatch"
msgstr "check_some_aliasing(): Невідповідність списку"

#: fortran/interface.c:3422
#, gcc-internal-format
msgid "check_some_aliasing(): corrupted data"
msgstr "check_some_aliasing(): пошкоджені дані"

#: fortran/interface.c:3432
#, gcc-internal-format
msgid ""
"Same actual argument associated with INTENT(%s) argument %qs and INTENT(%s) "
"argument %qs at %L"
msgstr ""
"Один і той самий фактичний аргумент асоційований з INTENT(%s) аргументом %qs "
"та INTENT(%s) аргументом %qs у %L"

#: fortran/interface.c:3462
#, gcc-internal-format
msgid "check_intents(): List mismatch"
msgstr "check_intents(): Невідповідність списку"

#: fortran/interface.c:3482
#, gcc-internal-format, gfc-internal-format
msgid ""
"Procedure argument at %L is local to a PURE procedure and has the POINTER "
"attribute"
msgstr ""
"Аргумент процедури в %L є локальним в процедурі PURE та має атрибут POINTER"

#: fortran/interface.c:3494
#, gcc-internal-format, gfc-internal-format
msgid ""
"Coindexed actual argument at %L in PURE procedure is passed to an INTENT(%s) "
"argument"
msgstr ""
"Коіндексований фактичний аргумент в %L в процедурі PURE переданий в аргумент "
"INTENT(%s)"

#: fortran/interface.c:3504
#, gcc-internal-format, gfc-internal-format
msgid ""
"Coindexed actual argument at %L in PURE procedure is passed to a POINTER "
"dummy argument"
msgstr ""
"Коіндексований фактичний аргумент в %L в процедурі PURE переданий в "
"формальний аргумент POINTER"

#: fortran/interface.c:3515
#, gcc-internal-format
msgid ""
"Coindexed polymorphic actual argument at %L is passed polymorphic dummy "
"argument %qs"
msgstr ""
"Коіндексований поліморфний фактичний аргумент в %L переданий як поліморфний "
"формальний аргумент %qs"

#: fortran/interface.c:3543 fortran/interface.c:3553
#, gcc-internal-format
msgid "Procedure %qs called at %L is not explicitly declared"
msgstr "Процедура %qs викликана в %L не є явно декларованою"

#: fortran/interface.c:3866
#, gcc-internal-format
msgid "Procedure %qs called with an implicit interface at %L"
msgstr "Процедура %qs викликана з неявним інтерфейсом в %L"

#: fortran/interface.c:3563
#, gcc-internal-format
msgid ""
"The pointer object %qs at %L must have an explicit function interface or be "
"declared as array"
msgstr ""
"Вказівний об'єкт %qs в %L повинен мати явний інтерфейс функції або бути "
"декларований як масив"

#: fortran/interface.c:3571
#, gcc-internal-format
msgid ""
"The allocatable object %qs at %L must have an explicit function interface or "
"be declared as array"
msgstr ""
"Розміщуваний об'єкт %qs в %L повинен мати явний інтерфейс функції або бути "
"декларований як масив"

#: fortran/interface.c:3579
#, gcc-internal-format
msgid ""
"Allocatable function %qs at %L must have an explicit function interface"
msgstr ""
"Розміщувана функція %qs в %L  повинна мати явний функціональний інтерфейс"

#: fortran/interface.c:3589
#, gcc-internal-format
msgid "Keyword argument requires explicit interface for procedure %qs at %L"
msgstr ""
"Аргумент з ключовим словом потребує явного інтерфейсу для процедури %qs в %L"

#: fortran/interface.c:3598
#, gcc-internal-format, gfc-internal-format
msgid "Assumed-type argument %s at %L requires an explicit interface"
msgstr "Аргумент передбачуваного типу  %s в %L вимагає явного інтерфейсу"

#: fortran/interface.c:3611
#, gcc-internal-format
msgid ""
"Actual argument of LOCK_TYPE or with LOCK_TYPE component at %L requires an "
"explicit interface for procedure %qs"
msgstr ""
"Фактичний аргумент LOCK_TYPE або з компонентом LOCK_TYPE в %L вимагає явний "
"інтерфейс для процедури %qs"

#: fortran/interface.c:3624
#, gcc-internal-format
msgid ""
"Actual argument of EVENT_TYPE or with EVENT_TYPE component at %L requires an "
"explicit interface for procedure %qs"
msgstr ""
"Фактичний аргумент EVENT_TYPE або з компонентом EVENT_TYPE в %L вимагає "
"явний інтерфейс для процедури %qs"

#: fortran/interface.c:3633
#, gcc-internal-format, gfc-internal-format
msgid "MOLD argument to NULL required at %L"
msgstr "Аргумент MOLD в NULL обов'язковий в %L"

#: fortran/interface.c:3641
#, gcc-internal-format, gfc-internal-format
msgid "Assumed-rank argument requires an explicit interface at %L"
msgstr "Аргумент передбачуваного рангу потребує явного інтерфейсу в %L"

#: fortran/interface.c:3679
#, gcc-internal-format
msgid ""
"Procedure pointer component %qs called with an implicit interface at %L"
msgstr ""
"Компонент вказівника на процедуру %qs викликаний з неявним інтерфейсом в %L"

#: fortran/interface.c:3690
#, gcc-internal-format
msgid ""
"Keyword argument requires explicit interface for procedure pointer component "
"%qs at %L"
msgstr ""
"Аргумент з ключовим словом вимагає явний інтерфейс для компонента вказівника "
"на процедуру %qs в %L"

#: fortran/interface.c:3775
#, gcc-internal-format, gfc-internal-format
msgid ""
"MOLD= required in NULL() argument at %L: Ambiguity between specific "
"functions %s and %s"
msgstr ""
"MOLD= обов'язковий в NULL() аргументі в %L: Неоднозначність між конкретними "
"функціями %s та %s"

#: fortran/interface.c:3845
#, gcc-internal-format
msgid "Unable to find symbol %qs"
msgstr "Неможливо знайти символ %qs"

#: fortran/interface.c:4212
#, gcc-internal-format
msgid "Entity %qs at %L is already present in the interface"
msgstr "Сутність %qs в %L вже присутня в інтерфейсі"

#: fortran/interface.c:4329
#, gcc-internal-format
msgid "gfc_add_interface(): Bad interface type"
msgstr "gfc_add_interface(): Некоректний тип інтерфейсу"

#: fortran/interface.c:4423
#, gcc-internal-format
msgid "Can't overwrite GENERIC %qs at %L"
msgstr "Неможливо перезаписати GENERIC %qs в %L"

#: fortran/interface.c:4435
#, gcc-internal-format
msgid "%qs at %L overrides a procedure binding declared NON_OVERRIDABLE"
msgstr ""
"%qs в %L перекриває зв'язування процедури декларованої NON_OVERRIDABLE"

#: fortran/interface.c:4443
#, gcc-internal-format
msgid "%qs at %L must not be DEFERRED as it overrides a non-DEFERRED binding"
msgstr ""
"%qs в %L не повинно бути DEFERRED, тому що це перекриває не-DEFERRED "
"зв'язування"

#: fortran/interface.c:4451
#, gcc-internal-format
msgid "%qs at %L overrides a PURE procedure and must also be PURE"
msgstr "%qs в %L перекриває PURE-процедуру і також має бути PURE"

#: fortran/interface.c:4460
#, gcc-internal-format
msgid "%qs at %L overrides an ELEMENTAL procedure and must also be ELEMENTAL"
msgstr "%qs в %L перекриває ELEMENTAL-процедуру і також має бути ELEMENTAL"

#: fortran/interface.c:4466
#, gcc-internal-format
msgid ""
"%qs at %L overrides a non-ELEMENTAL procedure and must not be ELEMENTAL, "
"either"
msgstr ""
"%qs в %L перекриває не-ELEMENTAL-процедуру і також має бути не-ELEMENTAL "
"також"

#: fortran/interface.c:4475
#, gcc-internal-format
msgid "%qs at %L overrides a SUBROUTINE and must also be a SUBROUTINE"
msgstr "%qs в %L перекриває SUBROUTINE і також має бути SUBROUTINE"

#: fortran/interface.c:4486
#, gcc-internal-format
msgid "%qs at %L overrides a FUNCTION and must also be a FUNCTION"
msgstr "%qs в %L перевизначає FUNCTION і також має бути FUNCTION"

#: fortran/interface.c:4494
#, gcc-internal-format
msgid "Result mismatch for the overriding procedure %qs at %L: %s"
msgstr ""
"Невідповідність аргументів для процедури, що перевизначається, %qs в %L: %s"

#: fortran/interface.c:4505
#, gcc-internal-format
msgid "%qs at %L overrides a PUBLIC procedure and must not be PRIVATE"
msgstr "%qs в %L перекриває PUBLIC і не може бути PRIVATE"

#: fortran/interface.c:4535
#, gcc-internal-format
msgid ""
"Dummy argument %qs of %qs at %L should be named %qs as to match the "
"corresponding argument of the overridden procedure"
msgstr ""
"Формальний аргумент  %qs від %qs в %L повинен називатися  %qs щоб пасувати "
"відповідному аргументу перевизначеної процедури"

#: fortran/interface.c:4547
#, gcc-internal-format
msgid "Argument mismatch for the overriding procedure %qs at %L: %s"
msgstr ""
"Невідповідність аргументу для процедури, що перевизначається, %qs в %L: %s"

#: fortran/interface.c:4556
#, gcc-internal-format
msgid ""
"%qs at %L must have the same number of formal arguments as the overridden "
"procedure"
msgstr ""
"%qs в %L повинно мати таку саму кількість формальних аргументів, як і "
"перевизначена процедра"

#: fortran/interface.c:4565
#, gcc-internal-format
msgid "%qs at %L overrides a NOPASS binding and must also be NOPASS"
msgstr "%qs в %L перекриває NOPASS-зв'язування і повинен також бути NOPASS"

#: fortran/interface.c:4576
#, gcc-internal-format
msgid "%qs at %L overrides a binding with PASS and must also be PASS"
msgstr "%qs в %L перекриває зв'язування з PASS і повинен також бути PASS"

#: fortran/interface.c:4583
#, gcc-internal-format
msgid ""
"Passed-object dummy argument of %qs at %L must be at the same position as "
"the passed-object dummy argument of the overridden procedure"
msgstr ""
"Passed-object фіктивний аргумент від %qs в %L повинен бути на тій же "
"позиції, що і passed-object формальний аргумент перевизначеної процедури"

#: fortran/interface.c:4604
#, gcc-internal-format, gfc-internal-format
msgid "DTIO dummy argument at %L must be of type %s"
msgstr "DTIO формальний аргумент %L має належати до типу %s"

#: fortran/interface.c:4611
#, gcc-internal-format, gfc-internal-format
msgid "DTIO dummy argument at %L must be of KIND = %d"
msgstr "DTIO формальний аргумент в %L має бути KIND = %d"

#: fortran/interface.c:4618
#, gcc-internal-format, gfc-internal-format
msgid "DTIO dummy argument at %L must be a scalar"
msgstr "DTIO формальний аргумент в %L має бути скаляром"

#: fortran/interface.c:4622
#, gcc-internal-format, gfc-internal-format
msgid "DTIO dummy argument at %L must be an ASSUMED SHAPE ARRAY"
msgstr "DTIO формальний аргумент в %L має бути ASSUMED SHAPE ARRAY"

#: fortran/interface.c:4626
#, gcc-internal-format, gfc-internal-format
msgid "DTIO dummy argument at %L must have INTENT %s"
msgstr "DTIO формальний аргумент в %L має бути INTENT %s"

#: fortran/interface.c:4685 fortran/interface.c:4729
#, gcc-internal-format, gfc-internal-format
msgid "Alternate return at %L is not permitted in a DTIO procedure"
msgstr "Альтернативне повернення %L неприпустима в DTIO-процедурі"

#: fortran/interface.c:4697
#, gcc-internal-format
msgid "DTIO procedure %qs at %L must be a subroutine"
msgstr "DTIO процедура %qs в %L має бути підпроцедурою"

#: fortran/interface.c:4706
#, gcc-internal-format
msgid "Too few dummy arguments in DTIO procedure %qs at %L"
msgstr "Замало фіктивних аргументів в DTIO процедурі %qs в %L"

#: fortran/interface.c:4713
#, gcc-internal-format
msgid "Too many dummy arguments in DTIO procedure %qs at %L"
msgstr "Забагато фіктивних аргументів в DTIO процедурі %qs в %L"

#: fortran/intrinsic.c:196
#, gcc-internal-format, gfc-internal-format
msgid ""
"Variable with NO_ARG_CHECK attribute at %L is only permitted as argument to "
"the intrinsic functions C_LOC and PRESENT"
msgstr ""
"Змінна з атрибутом NO_ARG_CHECK в %L лише дозволена як аргумент внутрішніх "
"функцій C_LOC та PRESENT"

#: fortran/intrinsic.c:211
#, gcc-internal-format, gfc-internal-format
msgid ""
"Assumed-type argument at %L is not permitted as actual argument to the "
"intrinsic %s"
msgstr ""
"Аргумент передбачуваного типу в %L не може бути фактичним аргументом "
"внутрішньої функції %s"

#: fortran/intrinsic.c:218
#, gcc-internal-format, gfc-internal-format
msgid ""
"Assumed-type argument at %L is only permitted as first actual argument to "
"the intrinsic %s"
msgstr ""
"Аргумент передбачуваного типу в %L може бути лише першим фактичним "
"аргументом внутрішньої функції %s"

#: fortran/intrinsic.c:225
#, gcc-internal-format, gfc-internal-format
msgid ""
"Assumed-rank argument at %L is only permitted as actual argument to "
"intrinsic inquiry functions"
msgstr ""
