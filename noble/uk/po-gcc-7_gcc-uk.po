#: ada/gcc-interface/lang-specs.h:66
msgid "-c required for gnat2scil"
msgstr "-c необхідний для gnat2scil"

#: fortran/lang-specs.h:60 fortran/lang-specs.h:74
msgid "gfortran does not support -E without -cpp"
msgstr "gfortran не підтримує -E без -cpp"

#: objc/lang-specs.h:30 objc/lang-specs.h:41
msgid "GNU Objective C no longer supports traditional compilation"
msgstr "GNU Objective C більше не підтримує традиційне компілювання"

#: objc/lang-specs.h:55
msgid ""
"objc-cpp-output is deprecated; please use objective-c-cpp-output instead"
msgstr ""
"objc-cpp-output вважається застарілим; будь ласка, скористайтеся замість "
"нього objective-c-cpp-output"

#: objcp/lang-specs.h:58
msgid ""
"objc++-cpp-output is deprecated; please use objective-c++-cpp-output instead"
msgstr ""
"objc++-cpp-output вважається застарілим; будь ласка, скористайтеся замість "
"нього objective-c++-cpp-output"

#: fortran/lang.opt:146
#, no-c-format
msgid "-J<directory>\tPut MODULE files in 'directory'."
msgstr "-J<каталог>\tПомістити файли MODULE в 'каталог'."

#: fortran/lang.opt:198
#, no-c-format
msgid "Warn about possible aliasing of dummy arguments."
msgstr "Попереджати про можливе перекриття формальних аргументів."

#: fortran/lang.opt:202
#, no-c-format
msgid "Warn about alignment of COMMON blocks."
msgstr "Попереджати про вирівнювання COMMON-блоків"

#: fortran/lang.opt:206
#, no-c-format
msgid "Warn about missing ampersand in continued character constants."
msgstr ""
"Попереджати про відсутніх символів & в символьних константах з продовженням."

#: fortran/lang.opt:210
#, no-c-format
msgid "Warn about creation of array temporaries."
msgstr "Попереджати щодо створення тимчасових масивів."

#: fortran/lang.opt:214
msgid "Warn about type and rank mismatches between arguments and parameters."
msgstr ""
"Попереджати про невідповідність типу та рангу між аргументами та параметрами."

#: fortran/lang.opt:218
#, no-c-format
msgid "Warn if the type of a variable might be not interoperable with C."
msgstr "Попереджати, якщо тип змінної може бути не сумісним зі С."

#: fortran/lang.opt:226
#, no-c-format
msgid "Warn about truncated character expressions."
msgstr "Попереджати про зрізані символьні вирази."

#: fortran/lang.opt:230
#, no-c-format
msgid ""
"Warn about equality comparisons involving REAL or COMPLEX expressions."
msgstr ""
"Попереджати про порівняння рівностей, що включають вирази REAL або COMPLEX."

#: fortran/lang.opt:238
#, no-c-format
msgid "Warn about most implicit conversions."
msgstr "Попереджати про найбільш імовірні перетворення."

#: fortran/lang.opt:254
#, no-c-format
msgid "Warn about function call elimination."
msgstr "Попереджати про видалення викликів функцій."

#: fortran/lang.opt:258
#, no-c-format
msgid "Warn about calls with implicit interface."
msgstr "Попереджати про виклики з неявним інтерфейсом."

#: fortran/lang.opt:262
#, no-c-format
msgid "Warn about called procedures not explicitly declared."
msgstr "Попереджати про виклики функцій, які не задекларовані явно."

#: fortran/lang.opt:266
#, no-c-format
msgid "Warn about constant integer divisions with truncated results."
msgstr ""
"Попереджати про ділення цілочисельних констант з скороченими результатами."

#: fortran/lang.opt:262
msgid "Warn about truncated source lines."
msgstr "Пропереджати про зрізані рядки джерельного коду."

#: fortran/lang.opt:274
#, no-c-format
msgid "Warn on intrinsics not part of the selected standard."
msgstr ""
"Попереджати про вбудовані функції, що не входять в заданий стандарт мови."

#: fortran/lang.opt:286
#, no-c-format
msgid "Warn about USE statements that have no ONLY qualifier."
msgstr "Попереджати про оператори USE, які не мають кваліфікатора ONLY."

#: fortran/lang.opt:302
#, no-c-format
msgid "Warn about real-literal-constants with 'q' exponent-letter."
msgstr "Попереджати про дійсні константи з експонентою, заданою літерою 'q'."

#: fortran/lang.opt:306
#, no-c-format
msgid "Warn when a left-hand-side array variable is reallocated."
msgstr ""
"Попереджати, коли змінній-масиву лівої частини виділено пам'ять заново."

#: fortran/lang.opt:310
#, no-c-format
msgid "Warn when a left-hand-side variable is reallocated."
msgstr "Попереджати, коли змінній лівої частини виділено пам'ять заново."

#: fortran/lang.opt:314
#, no-c-format
msgid "Warn if the pointer in a pointer assignment might outlive its target."
msgstr ""
"Попереджати, якщо вказівник у присвоєнні вказівника може пережити ціль."

#: fortran/lang.opt:322
#, no-c-format
msgid "Warn about \"suspicious\" constructs."
msgstr "Попереджати про «підозрілі» конструкції."

#: fortran/lang.opt:326
#, no-c-format
msgid "Permit nonconforming uses of the tab character."
msgstr "Дозволити неправильне вживання символів табуляції."

#: fortran/lang.opt:330
#, no-c-format
msgid "Warn about an invalid DO loop."
msgstr "Попереджати про неправильний цикл DO."

#: fortran/lang.opt:334
#, no-c-format
msgid "Warn about underflow of numerical constant expressions."
msgstr ""
"Попереджати про втрату значущості при обчисленні константних виразів."

#: fortran/lang.opt:342
#, no-c-format
msgid "Warn if a user-procedure has the same name as an intrinsic."
msgstr ""
"Попереджати про користувацькі процедури з назвами, що збігаються з назвами "
"вбудованих процедур."

#: fortran/lang.opt:350
#, no-c-format
msgid "Warn about unused dummy arguments."
msgstr "Попереджати про невикористовувані формальні аргументи."

#: fortran/lang.opt:354
#, no-c-format
msgid "Warn about zero-trip DO loops."
msgstr "Попереджати про цикли DO нульової довжини."

#: fortran/lang.opt:358
#, no-c-format
msgid "Enable preprocessing."
msgstr "Увімкнути препроцесинг."

#: fortran/lang.opt:366
#, no-c-format
msgid "Disable preprocessing."
msgstr "Вимкнути препроцесинг."

#: fortran/lang.opt:378
#, no-c-format
msgid "Eliminate multiple function invocations also for impure functions."
msgstr "Виключити повторні виклики також для не-pure функцій."

#: fortran/lang.opt:382
#, no-c-format
msgid "Enable alignment of COMMON blocks."
msgstr "Увімкнути вирівнювання COMMON-блоків"

#: fortran/lang.opt:386
#, no-c-format
msgid ""
"All intrinsics procedures are available regardless of selected standard."
msgstr ""
"Вважати, що доступні всі внутрішні процедури, незалежно від стандарту."

#: fortran/lang.opt:398
#, no-c-format
msgid ""
"Do not treat local variables and COMMON blocks as if they were named in SAVE "
"statements."
msgstr ""
"Не трактувати локальні змінні і COMMON-блоки так якби вони були перераховані "
"в операторах SAVE."

#: fortran/lang.opt:402
#, no-c-format
msgid "Specify that backslash in string introduces an escape character."
msgstr "Використовувати обернену косу риску в рядках як символ екранування."

#: fortran/lang.opt:406
#, no-c-format
msgid "Produce a backtrace when a runtime error is encountered."
msgstr "При помилках виконання виводити послідовність викликів."

#: fortran/lang.opt:410
#, no-c-format
msgid ""
"-fblas-matmul-limit=<n>\tSize of the smallest matrix for which matmul will "
"use BLAS."
msgstr ""
"-fblas-matmul-limit=<n>\tРозмір мінімальної матриці, для якої matmul буде "
"використовувати BLAS\r\n"
"."

#: fortran/lang.opt:414
#, no-c-format
msgid ""
"Produce a warning at runtime if a array temporary has been created for a "
"procedure argument."
msgstr ""
"Генерувати runtime-попередження, якщо для аргументу процедури був створений "
"тимчасовий масив."

#: fortran/lang.opt:398
msgid ""
"-fconvert=<big-endian|little-endian|native|swap> The endianness used for "
"unformatted files."
msgstr ""
"-fconvert=<big-endian|little-endian|native|swap> Порядок байтів, "
"використовуваний для неформатованих файлів."

#: fortran/lang.opt:401
#, c-format
msgid "Unrecognized option to endianness value: %qs"
msgstr "Нерозпізнана опція для значення типу endian: %qs"

#: fortran/lang.opt:437
#, no-c-format
msgid "Use the Cray Pointer extension."
msgstr "Використовувати розширення вказівників Cray"

#: fortran/lang.opt:449
#, no-c-format
msgid "Ignore 'D' in column one in fixed form."
msgstr "Ігнорувати 'D' в першій колонці в фіксованому форматі."

#: fortran/lang.opt:453
#, no-c-format
msgid "Treat lines with 'D' in column one as comments."
msgstr "Трактувати рядки з 'D' в першій колонці як коментарі."

#: fortran/lang.opt:461
#, no-c-format
msgid "Enable all DEC language extensions."
msgstr "Увімкнути всі розширення мови DEC"

#: fortran/lang.opt:482
#, no-c-format
msgid "Enable kind-specific variants of integer intrinsic functions."
msgstr "Увімкнути типові варіанти цілочисельних внутрішніх функцій."

#: fortran/lang.opt:486
#, no-c-format
msgid "Enable legacy math intrinsics for compatibility."
msgstr "Увімкнути успадковані вбудовані математичні функції для сумісності."

#: fortran/lang.opt:490
#, no-c-format
msgid "Enable support for DEC STRUCTURE/RECORD."
msgstr "Увмкнути підтримку для DEC STRUCTURE/RECORD."

#: fortran/lang.opt:494
#, no-c-format
msgid "Enable DEC-style STATIC and AUTOMATIC attributes."
msgstr "Увімкнути DEC-стиль для атрибутів STATIC and AUTOMATIC."

#: fortran/lang.opt:498
#, no-c-format
msgid "Set the default double precision kind to an 8 byte wide type."
msgstr ""
"Встановити за замовчуванням розмір 8 байт для типів double precision."

#: fortran/lang.opt:502
#, no-c-format
msgid "Set the default integer kind to an 8 byte wide type."
msgstr "Встановити за замовчуванням розмір 8 байт для цілочисельних типів."

#: fortran/lang.opt:506
#, no-c-format
msgid "Set the default real kind to an 8 byte wide type."
msgstr "Встановити за замовчуванням розмір 8 байт для дійсних типів."

#: fortran/lang.opt:518
#, no-c-format
msgid "Allow dollar signs in entity names."
msgstr "Дозволити використання символу $ в іменах сутностей."

#: fortran/lang.opt:214 fortran/lang.opt:522 config/alpha/alpha.opt:31
#: common.opt:653 common.opt:791 common.opt:1021 common.opt:1025
#: common.opt:1029 common.opt:1033 common.opt:1683 common.opt:1739
#: common.opt:1878 common.opt:1882 common.opt:2120 common.opt:2334
#: common.opt:3050
#, no-c-format
msgid "Does nothing. Preserved for backward compatibility."
msgstr "Ігнорується. Збережений для зворотньої сумісності."

#: fortran/lang.opt:526
#, no-c-format
msgid "Display the code tree after parsing."
msgstr "Показати дерево коду після синтаксичного розбору."

#: fortran/lang.opt:530
#, no-c-format
msgid "Display the code tree after front end optimization."
msgstr "Показати дерево коду після оптимізації клієнтської частини."

#: fortran/lang.opt:538
#, no-c-format
msgid "Display the code tree after parsing; deprecated option."
msgstr "Показати дерево коду після синтаксичного розбору; застарілий ключ."

#: fortran/lang.opt:542
#, no-c-format
msgid ""
"Specify that an external BLAS library should be used for matmul calls on "
"large-size arrays."
msgstr ""
"Задати використання зовнішньої бібліотеки BLAS для викликів matmul на "
"великих масивах."

#: fortran/lang.opt:546
#, no-c-format
msgid "Use f2c calling convention."
msgstr "Використовувати угоди f2c про виклики"

#: fortran/lang.opt:550
#, no-c-format
msgid "Assume that the source file is fixed form."
msgstr "Вважати, що вхідний файл має фіксований формат."

#: fortran/lang.opt:554
#, no-c-format
msgid ""
"Force creation of temporary to test infrequently-executed forall code."
msgstr ""
"Примусове створення тимчасових змінних для  випробування рідковиконуваного "
"forall."

#: fortran/lang.opt:558
#, no-c-format
msgid "Interpret any INTEGER(4) as an INTEGER(8)."
msgstr "Інтерпретувати INTEGER(4) як INTEGER(8)"

#: fortran/lang.opt:562 fortran/lang.opt:566
#, no-c-format
msgid "Specify where to find the compiled intrinsic modules."
msgstr ""
"Вказати розташування скомпільованих модулів з вбудованими процедурами."

#: fortran/lang.opt:570
#, no-c-format
msgid "Allow arbitrary character line width in fixed mode."
msgstr "Дозволити довільну ширину рядків у фіксованому форматі."

#: fortran/lang.opt:574
#, no-c-format
msgid "-ffixed-line-length-<n>\tUse n as character line width in fixed mode."
msgstr ""
"-ffixed-line-length-<n>\tВикористовувати n як ширину рядка у фіксованому "
"форматі."

#: fortran/lang.opt:582
#, no-c-format
msgid "-ffpe-trap=[...]\tStop on following floating point exceptions."
msgstr ""
"-ffpe-trap=[...]\tЗупинитися при зазначених винятках обчислення з рухомою "
"комою."

#: fortran/lang.opt:586
#, no-c-format
msgid "-ffpe-summary=[...]\tPrint summary of floating point exceptions."
msgstr ""
"-ffpe-summary=[...]\tВиведення підсумку винятків обчислення з рухомою комою."

#: fortran/lang.opt:590
#, no-c-format
msgid "Assume that the source file is free form."
msgstr "Вважати, що вихідний файл має вільну форму."

#: fortran/lang.opt:594
#, no-c-format
msgid "Allow arbitrary character line width in free mode."
msgstr "Допускати довільну ширину рядка у вільному форматі."

#: fortran/lang.opt:598
#, no-c-format
msgid "-ffree-line-length-<n>\tUse n as character line width in free mode."
msgstr ""
"-ffree-line-length-<n>\tВикористовувати n як ширину рядка у вільному форматі."

#: fortran/lang.opt:606
#, no-c-format
msgid "Enable front end optimization."
msgstr "Увімкнути оптимізацію клієнтської частини."

#: fortran/lang.opt:610
#, no-c-format
msgid ""
"Specify that no implicit typing is allowed, unless overridden by explicit "
"IMPLICIT statements."
msgstr ""
"Заборонити неявне визначення типів, якщо не задані явно оператори IMPLICIT."

#: fortran/lang.opt:614
#, no-c-format
msgid ""
"-finit-character=<n>\tInitialize local character variables to ASCII value n."
msgstr ""
"-finit-character=<n>\tІніціалізувати локальні символьні змінні ASCII "
"значенням n."

#: fortran/lang.opt:618
#, no-c-format
msgid ""
"Initialize components of derived type variables according to other init "
"flags."
msgstr ""
"Ініціалізувати компоненти змінних похідного типу відповідно до інших "
"прапорців ініціалізації."

#: fortran/lang.opt:622
#, no-c-format
msgid "-finit-integer=<n>\tInitialize local integer variables to n."
msgstr ""
"-finit-integer=<n>\tІніціалізувати локальні цілочисельні змінні значенням n."

#: fortran/lang.opt:626
#, no-c-format
msgid "Initialize local variables to zero (from g77)."
msgstr "Ініціалізувати локальні змінні нулем (з g77)."

#: fortran/lang.opt:630
#, no-c-format
msgid "-finit-logical=<true|false>\tInitialize local logical variables."
msgstr "-finit-logical=<true|false>\tІніціалізувати локальні логічні змінні."

#: fortran/lang.opt:634
#, no-c-format
msgid ""
"-finit-real=<zero|snan|nan|inf|-inf>\tInitialize local real variables."
msgstr ""
"-finit-real=<zero|snan|nan|inf|-inf>\tІніціалізувати локальні дійсні змінні."

#: fortran/lang.opt:568
#, c-format
msgid "Unrecognized option to floating-point init value: %qs"
msgstr ""
"Нерозпізнаний ключ для значення ініціалізації змінної з рухомою комою: %qs"

#: fortran/lang.opt:660
#, no-c-format
msgid ""
"-finline-matmul-limit=<n>\tSpecify the size of the largest matrix for which "
"matmul will be inlined."
msgstr ""
"-finline-matmul-limit=<n>\tРозмір максимальної матриці, для якої matmul буде "
"вбудований."

#: fortran/lang.opt:664
#, no-c-format
msgid ""
"-fmax-array-constructor=<n>\tMaximum number of objects in an array "
"constructor."
msgstr ""
"-fmax-array-constructor=<n>\tМаксимальне число об'єктів в конструкторі "
"масива."

#: fortran/lang.opt:668
#, no-c-format
msgid "-fmax-identifier-length=<n>\tMaximum identifier length."
msgstr "-fmax-identifier-length=<n>\tМаксимальна довжина ідентифікатора."

#: fortran/lang.opt:672
#, no-c-format
msgid "-fmax-subrecord-length=<n>\tMaximum length for subrecords."
msgstr "-fmax-subrecord-length=<n>\tМаксимальна довжина субзаписів."

#: fortran/lang.opt:676
#, no-c-format
msgid ""
"-fmax-stack-var-size=<n>\tSize in bytes of the largest array that will be "
"put on the stack."
msgstr ""
"-fmax-stack-var-size=<n>\tРозмір в байтах максимального масиву, що "
"розміщується в стеку."

#: fortran/lang.opt:680
#, no-c-format
msgid "Put all local arrays on stack."
msgstr "Помістити усі локальні масиви в стек."

#: fortran/lang.opt:684
#, no-c-format
msgid "Set default accessibility of module entities to PRIVATE."
msgstr ""
"Встановити PRIVATE як типове значення для атрибуту доступності сутностей "
"модуля."

#: fortran/lang.opt:704
#, no-c-format
msgid "Try to lay out derived types as compactly as possible."
msgstr "Спробувати використовувати найкомпактніше подання похідних типів."

#: fortran/lang.opt:712
#, no-c-format
msgid "Protect parentheses in expressions."
msgstr "Захищати дужки у виразах."

#: fortran/lang.opt:720
#, no-c-format
msgid "Enable range checking during compilation."
msgstr "Увімкнути перевірку діапазонів при компілюванні."

#: fortran/lang.opt:724
#, no-c-format
msgid "Interpret any REAL(4) as a REAL(8)."
msgstr "Інтерпретувати усі REAL(4) як REAL(8)."

#: fortran/lang.opt:728
#, no-c-format
msgid "Interpret any REAL(4) as a REAL(10)."
msgstr "Інтерпретувати усі REAL(4) як REAL(10)."

#: fortran/lang.opt:732
#, no-c-format
msgid "Interpret any REAL(4) as a REAL(16)."
msgstr "Інтерпретувати усі REAL(4) як REAL(16)."

#: fortran/lang.opt:736
#, no-c-format
msgid "Interpret any REAL(8) as a REAL(4)."
msgstr "Інтерпретувати усі REAL(8) як REAL(4)."

#: fortran/lang.opt:740
#, no-c-format
msgid "Interpret any REAL(8) as a REAL(10)."
msgstr "Інтерпретувати усі REAL(8) як REAL(10)."

#: fortran/lang.opt:744
#, no-c-format
msgid "Interpret any REAL(8) as a REAL(16)."
msgstr "Інтерпретувати усі REAL(8) як REAL(16)."

#: fortran/lang.opt:748
#, no-c-format
msgid "Reallocate the LHS in assignments."
msgstr "Виділяти пам'ять заново для лівих частин присвоювання."

#: fortran/lang.opt:752
#, no-c-format
msgid "Use a 4-byte record marker for unformatted files."
msgstr "Використовувати 4-байтовий маркер записів в неформатованих файлах."

#: fortran/lang.opt:756
#, no-c-format
msgid "Use an 8-byte record marker for unformatted files."
msgstr "Використовувати 8-байтовий маркер записів в неформатованих файлах."

#: fortran/lang.opt:760
#, no-c-format
msgid "Allocate local variables on the stack to allow indirect recursion."
msgstr "Розміщувати локальні змінні в стеку для підтримки неявної рекурсії."

#: fortran/lang.opt:764
#, no-c-format
msgid "Copy array sections into a contiguous block on procedure entry."
msgstr "При вході в процедуру копіювати секції масивів в послідовний блок."

#: fortran/lang.opt:768
#, no-c-format
msgid ""
"-fcoarray=<none|single|lib>\tSpecify which coarray parallelization should be "
"used."
msgstr ""
"-fcoarray=<none|single|lib>\tВкажіть спосіб coarray-розпаралелювання."

#: fortran/lang.opt:771
#, gcc-internal-format
msgid "Unrecognized option: %qs"
msgstr "Нерозпізнаний ключ: %qs"

#: fortran/lang.opt:784
#, no-c-format
msgid "-fcheck=[...]\tSpecify which runtime checks are to be performed."
msgstr "-fcheck=[...]\tВкажіть набір виконуваних runtime-перевірок."

#: fortran/lang.opt:788
#, no-c-format
msgid ""
"Append a second underscore if the name already contains an underscore."
msgstr ""
"Додавати другий символ підкреслення до імен, які вже містять підкреслення."

#: fortran/lang.opt:796
#, no-c-format
msgid "Apply negative sign to zero values."
msgstr "Застосовувати знак мінус до нульових значень."

#: fortran/lang.opt:807
#, no-c-format
msgid "Append underscores to externally visible names."
msgstr "Додавати підкреслення до доступних ззовні імен."

#: fortran/lang.opt:811 c-family/c.opt:1539 c-family/c.opt:1571
#: c-family/c.opt:1579 c-family/c.opt:1890 config/pa/pa.opt:42
