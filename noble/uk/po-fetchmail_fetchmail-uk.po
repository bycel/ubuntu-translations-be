msgstr "  -I, --interface потрібна для інтерфейсу специфікація\n"

#: options.c:665
msgid "  -M, --monitor     monitor interface for activity\n"
msgstr "  -M, --monitor стеження за активністю інтерфейсу\n"

#: options.c:668
msgid "      --ssl         enable ssl encrypted session\n"
msgstr "      --ssl увімкнення сесії, зашифрованої ssl\n"

#: options.c:669
msgid "      --sslkey      ssl private key file\n"
msgstr "      --sslkey файл секретного ключа ssl\n"

#: options.c:670
msgid "      --sslcert     ssl client certificate\n"
msgstr "      --sslcert сертифікат клієнта ssl\n"

