"\t\t[-v|--vertical-label string] [-w|--width пікселі]\n"
"\t\t[--right-axis scale:shift] [--right-axis-label мітка]\n"
"\t\t[--right-axis-format формат]\n"
"\t\t[-h|--height пікселі] [-o|--logarithmic]\n"
"\t\t[-u|--upper-limit значення] [-z|--lazy]\n"
"\t\t[-l|--lower-limit значення] [-r|--rigid]\n"
"\t\t[-g|--no-legend] [-d|--daemon <address>]\n"
"\t\t[-F|--force-rules-legend]\n"
"\t\t[-j|--only-graph]\n"

