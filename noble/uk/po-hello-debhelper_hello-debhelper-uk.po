#. TRANSLATORS: --help output 1 (synopsis)
#. no-wrap
#: src/hello.c:148
#, c-format
msgid "Usage: %s [OPTION]...\n"
msgstr "Використання: %s [КЛЮЧ]...\n"

#. TRANSLATORS: --help output 2 (brief description)
#. no-wrap
#: src/hello.c:153
msgid "Print a friendly, customizable greeting.\n"
msgstr "Вивести дружнє, налаштовуване вітання.\n"

#. TRANSLATORS: --help output 3: options 1/2
#. no-wrap
#: src/hello.c:159
msgid ""
"  -h, --help          display this help and exit\n"
"  -v, --version       display version information and exit\n"
msgstr ""
"  -h, --help          показати підказку та вийти\n"
"  -v, --version       показати версію та вийти\n"

#. TRANSLATORS: --help output 4: options 2/2
#. no-wrap
#: src/hello.c:166
msgid ""
"  -t, --traditional       use traditional greeting format\n"
"  -n, --next-generation   use next-generation greeting format\n"
"  -g, --greeting=TEXT     use TEXT as the greeting message\n"
msgstr ""
"  -t, --traditional       використовувати традиційне представлення "
"привітання\n"
"  -n, --next-generation   використовувати представлення привітання нового "
"покоління\n"
"  -g, --greeting=ТЕКСТ     використовувати ТЕКСТ як повідомлення для "
"привітання\n"

