msgid " * List connected ports (no subscription action)\n"
msgstr " * Список під’єднаних портів (не потрібна підписка)\n"

#: seq/aconnect/aconnect.c:60
#, c-format
msgid "   aconnect -i|-o [-options]\n"
msgstr "   aconnect -i|-o [-опції]\n"

#: seq/aconnect/aconnect.c:61
#, c-format
msgid "     -i,--input          list input (readable) ports\n"
msgstr "     -i,--input список (читанних) портів вводу\n"

#: seq/aconnect/aconnect.c:62
#, c-format
msgid "     -o,--output         list output (writable) ports\n"
msgstr "     -o,--output список портів виводу (з можливістю запису)\n"

