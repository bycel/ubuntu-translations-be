msgstr "%.1f ГБ"

#: glib/gutils.c:3229
#, c-format
msgid "%.1f TB"
msgstr "%.1f ТБ"

#: glib/gutils.c:3234
#, c-format
msgid "%.1f PB"
msgstr "%.1f ПБ"

#: glib/gutils.c:3239
#, c-format
msgid "%.1f EB"
msgstr "%.1f ЕБ"

