msgstr "Помилка з'єднання з поштовим сервером '%(server)s': %(reason)s"

msgid "Mail not sent"
msgstr "Пошту не відправлено"

msgid "Mail sent OK"
msgstr "Пошту відправлено"

msgid "Date"
msgstr "Дата"

msgid "From"
msgstr "Від"

msgid "To"
msgstr "До"

