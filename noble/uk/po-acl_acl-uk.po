#, c-format
msgid "%s: %s in line %d of file %s\n"
msgstr "%s: %s в лінії %d файлу %s\n"

#: tools/setfacl.c:540
#, c-format
msgid "%s: %s in line %d of standard input\n"
msgstr "%s: %s у рядку %d стандартного вводу\n"
