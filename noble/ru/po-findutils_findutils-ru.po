msgid ""
"\n"
"Tests (N can be +N or -N or N):\n"
"      -amin N -anewer FILE -atime N -cmin N -cnewer FILE -context CONTEXT\n"
"      -ctime N -empty -false -fstype TYPE -gid N -group NAME -ilname "
