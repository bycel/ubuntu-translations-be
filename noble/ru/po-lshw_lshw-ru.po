#: ../lshw.cc:24
#, c-format
msgid "usage: %s [-format] [-options ...]\n"
msgstr "использование: %s [-формат] [-параметры ...]\n"

#: ../lshw.cc:25
#, c-format
msgid "       %s -version\n"
msgstr "       %s -version\n"

#: ../lshw.cc:27
#, c-format
msgid "\t-version        print program version (%s)\n"
msgstr "\t-version        вывод версии программы (%s)\n"

#: ../lshw.cc:28
#, c-format
msgid ""
"\n"
"format can be\n"
msgstr ""
"\n"
"возможный формат:\n"

#: ../lshw.cc:29
#, c-format
msgid "\t-html           output hardware tree as HTML\n"
msgstr "\t-html           вывод дерева устройств в виде HTML\n"

#: ../lshw.cc:30
#, c-format
msgid "\t-xml            output hardware tree as XML\n"
msgstr "\t-xml            вывод дерева устройств в виде XML\n"

#: ../lshw.cc:31
#, c-format
msgid "\t-short          output hardware paths\n"
msgstr ""

#: ../lshw.cc:32
#, c-format
msgid "\t-businfo        output bus information\n"
msgstr "\t-businfo        вывод информации о системной шине\n"

#: ../lshw.cc:34
#, c-format
msgid "\t-X              use graphical interface\n"
msgstr "\t-X              использовать графический интерфейс\n"

#: ../lshw.cc:35
#, c-format
msgid ""
"\n"
"options can be\n"
msgstr ""
"\n"
"возможные параметры:\n"

#: ../lshw.cc:37
#, c-format
msgid "\t-class CLASS    only show a certain class of hardware\n"
msgstr "\t-class КЛАСС    только заданный класс устройств\n"

#: ../lshw.cc:38
#, c-format
msgid "\t-C CLASS        same as '-class CLASS'\n"
msgstr "\t-C КЛАСС        то же, что и '-class КЛАСС'\n"

#: ../lshw.cc:39
#, c-format
msgid "\t-c CLASS        same as '-class CLASS'\n"
msgstr "\t-c КЛАСС        то же, что и '-class КЛАСС'\n"

#: ../lshw.cc:41
#, c-format
msgid "\t-disable TEST   disable a test (like pci, isapnp, cpuid, etc. )\n"
msgstr ""
"\t-disable ТЕСТ   отключить тест (например, pci, isapnp, cpuid и т. п.)\n"

#: ../lshw.cc:43
#, c-format
msgid "\t-enable TEST    enable a test (like pci, isapnp, cpuid, etc. )\n"
msgstr ""
"\t-enable ТЕСТ    включить тест (например, pci, isapnp, cpuid и т. п.)\n"

#: ../lshw.cc:44
#, c-format
msgid "\t-quiet          don't display status\n"
msgstr "\t-quiet          не показывать статус\n"

#: ../lshw.cc:45
#, c-format
msgid ""
"\t-sanitize       sanitize output (remove sensitive information like serial "
"numbers, etc.)\n"
msgstr ""
"\t-sanitize       не выводить секретную информацию (серийные номера и т.п.)\n"

#: ../lshw.cc:46
#, c-format
msgid "\t-numeric        output numeric IDs (for PCI, USB, etc.)\n"
msgstr ""
"\t-numeric        выводить цифровые идентификаторы (для PCI, USB и т.п.)\n"

