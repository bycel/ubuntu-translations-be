#: src/xz/args.c:77
#, c-format
msgid "%s: Invalid argument to --block-list"
msgstr "%s: недопустимый аргумент для --block-list"

#: src/xz/args.c:87
#, c-format
msgid "%s: Too many arguments to --block-list"
msgstr "%s: слишком много аргументов для --block-list"

#: src/xz/args.c:124
#, c-format
msgid ""
"In --block-list, block size is missing after filter chain number '%c:'"
msgstr ""
"В --block-list отсутствует размер блока после номера цепочки фильтра '%c:'."

#: src/xz/args.c:150
msgid "0 can only be used as the last element in --block-list"
msgstr "0 может использоваться только как последний элемент в --block-list"

#: src/xz/args.c:539
#, c-format
msgid "%s: Unknown file format type"
msgstr "%s: неизвестный формат файла"

#: src/xz/args.c:562 src/xz/args.c:570
#, c-format
msgid "%s: Unsupported integrity check type"
msgstr "%s: неподдерживаемый тип проверки целостности"

#: src/xz/args.c:606
msgid "Only one file can be specified with '--files' or '--files0'."
msgstr "С помощью '--files' или '--files0' можно указать только один файл."

#. TRANSLATORS: This is a translatable
#. string because French needs a space
#. before the colon ("%s : %s").
