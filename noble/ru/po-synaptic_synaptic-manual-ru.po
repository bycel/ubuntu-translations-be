msgid "<guimenu>GNOME Applications</guimenu> menu"
msgstr "Меню GNOME <guimenu>Приложения</guimenu>"

#: ../help/C/synaptic.xml:166(para)
msgid ""
"Choose <menuchoice><guisubmenu>System "
"Tools</guisubmenu><guimenuitem>Synaptic Package "
