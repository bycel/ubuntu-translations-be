"\t\t[-B|--font-smoothing-threshold размер]\n"
"\t\t[-T|--tabwidth ширина]\n"
"\t\t[-E|--slope-mode]\n"
"\t\t[-P|--pango-markup]\n"
"\t\t[-N|--no-gridfit]\n"
"\t\t[-X|--units-exponent значение]\n"
"\t\t[-L|--units-length значение]\n"
