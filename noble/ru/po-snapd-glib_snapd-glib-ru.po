#: snapd-glib/snapd-interface.c:156
msgid "Add user accounts and change passwords"
msgstr "Добавление учётных записей пользователей и изменение паролей"

#: snapd-glib/snapd-interface.c:158 snapd-glib/snapd-interface.c:232
msgid "Play and record sound"
msgstr "Воспроизведение и запись звука"

#: snapd-glib/snapd-interface.c:160
msgid "Play audio"
msgstr "Воспроизведение звука"

#: snapd-glib/snapd-interface.c:162
msgid "Record audio"
msgstr "Запись звука"

#: snapd-glib/snapd-interface.c:164
msgid "Detect network devices using mDNS/DNS-SD (Bonjour/zeroconf)"
msgstr ""
"Определение сетевых устройств с использованием mDNS/DNS-SD (Bonjour/zeroconf)"

#: snapd-glib/snapd-interface.c:166
msgid "Access bluetooth hardware directly"
msgstr "Прямой доступ к bluetooth устройству"

#: snapd-glib/snapd-interface.c:168
msgid "Use bluetooth devices"
msgstr "Использование устройств bluetooth"

#: snapd-glib/snapd-interface.c:170
msgid "Use your camera"
msgstr "Использование камеры"

#: snapd-glib/snapd-interface.c:172
msgid "Print documents"
msgstr "Печать документов"

#: snapd-glib/snapd-interface.c:174
msgid "Use any connected joystick"
msgstr "Использование подключённых джойстиков"

#: snapd-glib/snapd-interface.c:176
msgid "Allow connecting to the Docker service"
msgstr "Разрешение подключения к службе Docker"

#: snapd-glib/snapd-interface.c:178
msgid "Configure network firewall"
msgstr "Настройка межсетевого экрана"

#: snapd-glib/snapd-interface.c:180
msgid "Setup and use privileged FUSE filesystems"
msgstr "Установка и использование привилегированных файловых систем FUSE"

#: snapd-glib/snapd-interface.c:182
msgid "Update firmware on this device"
msgstr "Обновление прошивки на этом устройстве"

#: snapd-glib/snapd-interface.c:184
msgid "Access hardware information"
msgstr "Доступ к сведениям об оборудовании"

#: snapd-glib/snapd-interface.c:186
msgid "Provide entropy to hardware random number generator"
msgstr "Обеспечение энтропии аппаратного генератора случайных чисел"

#: snapd-glib/snapd-interface.c:188
msgid "Use hardware-generated random numbers"
msgstr "Использование аппаратно сгенерированных случайных чисел"

#: snapd-glib/snapd-interface.c:190
msgid "Access files in your home folder"
msgstr "Доступ к файлам в вашей домашней папке"

#: snapd-glib/snapd-interface.c:192
msgid "Access libvirt service"
msgstr "Доступ к службе libvirt"

#: snapd-glib/snapd-interface.c:194
msgid "Change system language and region settings"
msgstr "Изменение языка системы и региональных настроек"

#: snapd-glib/snapd-interface.c:196
msgid "Change location settings and providers"
msgstr "Изменение настроек местоположения и поставщиков услуг"

#: snapd-glib/snapd-interface.c:198
msgid "Access your location"
msgstr "Доступ к вашему местоположению"

#: snapd-glib/snapd-interface.c:200
msgid "Read system and application logs"
msgstr "Чтение журналов событий системы и приложений"

#: snapd-glib/snapd-interface.c:202
msgid "Access LXD service"
msgstr "Доступ к службе LXD"

#: snapd-glib/snapd-interface.c:204
msgid "Use and configure modems"
msgstr "Использование и настройка модемов"

#: snapd-glib/snapd-interface.c:206
msgid "Read system mount information and disk quotas"
msgstr "Чтение сведений о точках подключений и дисковых квотах"

#: snapd-glib/snapd-interface.c:208
msgid "Control music and video players"
msgstr "Управление музыкальными и видео проигрывателями"

#: snapd-glib/snapd-interface.c:210
msgid "Change low-level network settings"
msgstr "Изменение низкоуровневых настроек сети"

#: snapd-glib/snapd-interface.c:212
msgid "Access the NetworkManager service to read and change network settings"
msgstr ""
"Доступ к службе NetworkManager для чтения и изменения сетевых настроек"

#: snapd-glib/snapd-interface.c:214
msgid "Read access to network settings"
msgstr "Доступ к чтению сетевых настроек"

#: snapd-glib/snapd-interface.c:216
msgid "Change network settings"
msgstr "Изменение сетевых настроек"

#: snapd-glib/snapd-interface.c:218
msgid "Read network settings"
msgstr "Чтение сетевых настроек"

#: snapd-glib/snapd-interface.c:220
msgid ""
"Access the ofono service to read and change network settings for mobile "
"telephony"
msgstr ""
"Доступ к службе ofono, для чтения и изменения параметров сети при "
"использовании мобильной телефонии"

#: snapd-glib/snapd-interface.c:222
msgid "Control Open vSwitch hardware"
msgstr "Управление оборудованием Open vSwitch"

#: snapd-glib/snapd-interface.c:224
msgid "Read from CD/DVD"
msgstr "Чтение с CD/DVD"

#: snapd-glib/snapd-interface.c:226
msgid "Read, add, change, or remove saved passwords"
msgstr "Чтение, добавление, изменение или удаление сохранённых паролей"

#: snapd-glib/snapd-interface.c:228
msgid ""
"Access pppd and ppp devices for configuring Point-to-Point Protocol "
"connections"
msgstr ""
"Доступ к pppd и ppp устройствам для настройки соединений по протоколу Point-"
"to-Point"

#: snapd-glib/snapd-interface.c:230
msgid "Pause or end any process on the system"
msgstr "Приостановление или завершение любого процесса системы"

#: snapd-glib/snapd-interface.c:234
msgid "Access USB hardware directly"
msgstr "Прямой доступ к USB оборудованию"

#: snapd-glib/snapd-interface.c:236
msgid "Read/write files on removable storage devices"
msgstr "Чтение и запись файлов на съёмных устройствах хранения"

#: snapd-glib/snapd-interface.c:238
msgid "Prevent screen sleep/lock"
msgstr "Предотвращение блокирования и перехода экрана в спящий режим"

#: snapd-glib/snapd-interface.c:240
msgid "Access serial port hardware"
msgstr "Доступ к обрудованию с последовательными портами"

#: snapd-glib/snapd-interface.c:242
msgid "Restart or power off the device"
msgstr "Перезапуск или выключение устройства"

#: snapd-glib/snapd-interface.c:244
msgid "Install, remove and configure software"
msgstr "Установка, удаление и настройка ПО"

#: snapd-glib/snapd-interface.c:246
msgid "Access Storage Framework service"
msgstr "Служба доступа к оснастке хранилища"

#: snapd-glib/snapd-interface.c:248
msgid "Read process and system information"
msgstr "Чтение сведений о системе и процессах"

#: snapd-glib/snapd-interface.c:250
msgid "Monitor and control any running program"
msgstr "Мониторинг и управление любой выполняющейся программой"

#: snapd-glib/snapd-interface.c:252
msgid "Change the date and time"
msgstr "Изменение даты и времени"

#: snapd-glib/snapd-interface.c:254
msgid "Change time server settings"
msgstr "Изменение настроек сервера точного времени"

#: snapd-glib/snapd-interface.c:256
msgid "Change the time zone"
msgstr "Изменение часовой зоны"

#: snapd-glib/snapd-interface.c:258
msgid "Access the UDisks2 service for configuring disks and removable media"
msgstr ""
"Доступ к службе UDisks2 для конфигурирования дисков и съёмных накопителей"

#: snapd-glib/snapd-interface.c:260
msgid "Read/change shared calendar events in Ubuntu Unity 8"
msgstr "Чтение и изменение общедоступных событий календаря в Ubuntu Unity 8"

#: snapd-glib/snapd-interface.c:262
msgid "Read/change shared contacts in Ubuntu Unity 8"
msgstr "Чтение и изменение общедоступных контактных данных в Ubuntu Unity 8"

#: snapd-glib/snapd-interface.c:264
msgid "Access energy usage data"
msgstr "Доступ к сведениям об энергопотреблении"

#: snapd-glib/snapd-interface.c:266
msgid "Read/write access to U2F devices exposed"
msgstr "Доступ чтения и записи к открытым устройствам U2F"
